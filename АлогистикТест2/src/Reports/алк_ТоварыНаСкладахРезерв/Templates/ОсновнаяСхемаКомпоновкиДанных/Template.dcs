	МИНИМУМ(алк_ОстаткиЗапасовОбороты.ПериодСекунда) КАК ПериодСекунда,
	алк_ОстаткиЗапасовОбороты.СерийныйНомер КАК СерийныйНомер,
	СУММА(алк_ОстаткиЗапасовОбороты.КоличествоПриход) КАК КоличествоПриход
ПОМЕСТИТЬ ВТ_ДатаУпаковки
ИЗ
	РегистрНакопления.алк_ОстаткиЗапасов.Обороты(
			,
			,
			Авто,
			СерийныйНомер В
					(ВЫБРАТЬ
						Остатки.СерийныйНомер КАК СерийныйНомер
					ИЗ
						Остатки КАК Остатки)
				И СтруктурнаяЕдиница В
					(ВЫБРАТЬ
						СтруктурныеЕдиницы.Ссылка КАК Ссылка
					ИЗ
						Справочник.СтруктурныеЕдиницы КАК СтруктурныеЕдиницы) {(СерийныйНомер В
			        (ВЫБРАТЬ
			            Остатки.СерийныйНомер КАК СерийныйНомер
			        ИЗ
			            Остатки КАК Остатки)
			    И СтруктурнаяЕдиница В
			        (ВЫБРАТЬ
			            СтруктурныеЕдиницы.Ссылка КАК Ссылка
			        ИЗ
			            Справочник.СтруктурныеЕдиницы КАК СтруктурныеЕдиницы)) КАК ОтбрНеНужен}) КАК алк_ОстаткиЗапасовОбороты

СГРУППИРОВАТЬ ПО
	алк_ОстаткиЗапасовОбороты.СерийныйНомер
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_Итог.СтруктурнаяЕдиница КАК СтруктурнаяЕдиница,
	ВТ_Итог.СерийныйНомер КАК СерийныйНомер,
	ВТ_Итог.Штабель КАК Штабель,
	ВТ_Итог.КоличествоСостав КАК КоличествоСостав,
	ВТ_Итог.Сертификат КАК Сертификат,
	ВТ_Итог.Номенклатура КАК Номенклатура,
	ВТ_Итог.Характеристика КАК Характеристика,
	ВТ_Итог.Порода КАК Порода,
	ВТ_Итог.Сорт КАК Сорт,
	ВТ_Итог.Длина КАК Длина,
	ВТ_Итог.Сечение КАК Сечение,
	ВТ_Итог.Влажность КАК Влажность,
	ВЫБОР
		КОГДА ВТ_Итог.ЗаказПокупателя = ЗНАЧЕНИЕ(Документ.алк_ЗаказПокупателя.ПустаяСсылка)
			ТОГДА "БЕЗ ЗАКАЗА"
		КОГДА ВТ_Итог.ЗаказПокупателя = ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)
			ТОГДА "БЕЗ ЗАКАЗА"
		КОГДА ВТ_Итог.ЗаказПокупателя ССЫЛКА Документ.алк_ЗаказПокупателя
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(ВТ_Итог.ЗаказПокупателя КАК Документ.алк_ЗаказПокупателя).Закрыт
						ТОГДА "БЕЗ ЗАКАЗА"
					ИНАЧЕ ВТ_Итог.ЗаказПокупателя
				КОНЕЦ
		КОГДА ВТ_Итог.ЗаказПокупателя ССЫЛКА Документ.алк_Аддендумы
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(ВТ_Итог.ЗаказПокупателя КАК Документ.алк_Аддендумы).Статус = ЗНАЧЕНИЕ(Перечисление.алк_СтатусыАддендума.Закрыт)
						ТОГДА "БЕЗ ЗАКАЗА"
					ИНАЧЕ ВТ_Итог.ЗаказПокупателя
				КОНЕЦ
		ИНАЧЕ ВТ_Итог.ЗаказПокупателя
	КОНЕЦ КАК ЗаказПокупателя,
	ВТ_Итог.КатегорияНоменклатуры КАК КатегорияНоменклатуры,
	ВТ_Итог.ВесНеттоТаможня КАК ВесНеттоТаможня,
	ВТ_Итог.ВесБруттоТаможня КАК ВесБруттоТаможня,
	ВТ_Итог.БоковаяДоска КАК БоковаяДоска,
	ВТ_Итог.Объем КАК Объем,
	ВТ_Итог.Организация КАК Организация,
	ВТ_Итог.КоличествоПакетов КАК КоличествоПакетов,
	ВТ_Итог.СортДляСвязи КАК СортДляСвязи,
	ЕСТЬNULL(Расходные.Регистратор, ЗНАЧЕНИЕ(Документ.алк_РасходнаяНакладная.ПустаяСсылка)) КАК Расходная,
	ВТ_ДатаУпаковки.ПериодСекунда КАК ДатаУпаковки,
	ВЫБОР
		КОГДА ВТ_ДатаУпаковки.ПериодСекунда &lt; ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;ОпорнаяДата, ГОД), ГОД, -1)
			ТОГДА СТРЗАМЕНИТЬ(СТРОКА(ГОД(ВТ_ДатаУпаковки.ПериодСекунда)), " ", "") + " г"
		КОГДА ВТ_ДатаУпаковки.ПериодСекунда &lt; ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;ОпорнаяДата, ГОД), МЕСЯЦ, -6)
			ТОГДА "1 полугодие " + СТРЗАМЕНИТЬ(СТРОКА(ГОД(ВТ_ДатаУпаковки.ПериодСекунда)), " ", "") + " г"
		КОГДА ВТ_ДатаУпаковки.ПериодСекунда &lt; НАЧАЛОПЕРИОДА(&amp;ОпорнаяДата, ГОД)
			ТОГДА "2 полугодие " + СТРЗАМЕНИТЬ(СТРОКА(ГОД(ВТ_ДатаУпаковки.ПериодСекунда)), " ", "") + " г"
		КОГДА ВТ_ДатаУпаковки.ПериодСекунда &gt;= НАЧАЛОПЕРИОДА(&amp;ОпорнаяДата, ГОД)
			ТОГДА СТРОКА(КВАРТАЛ(ВТ_ДатаУпаковки.ПериодСекунда)) + " кв - " + СТРЗАМЕНИТЬ(СТРОКА(ГОД(ВТ_ДатаУпаковки.ПериодСекунда)), " ", "") + " г"
	КОНЕЦ КАК ПериодУпаковки,
	ВЫБОР
		КОГДА ВТ_Итог.ЗаказПокупателя = ЗНАЧЕНИЕ(Документ.алк_ЗаказПокупателя.ПустаяСсылка)
			ТОГДА ВТ_Итог.Объем
		КОГДА ВТ_Итог.ЗаказПокупателя = ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)
			ТОГДА ВТ_Итог.Объем
		КОГДА ВТ_Итог.ЗаказПокупателя ССЫЛКА Документ.алк_ЗаказПокупателя
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(ВТ_Итог.ЗаказПокупателя КАК Документ.алк_ЗаказПокупателя).Закрыт
						ТОГДА ВТ_Итог.Объем
					ИНАЧЕ 0
				КОНЕЦ
		КОГДА ВТ_Итог.ЗаказПокупателя ССЫЛКА Документ.алк_Аддендумы
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(ВТ_Итог.ЗаказПокупателя КАК Документ.алк_Аддендумы).Статус = ЗНАЧЕНИЕ(Перечисление.алк_СтатусыАддендума.Закрыт)
						ТОГДА ВТ_Итог.Объем
					ИНАЧЕ 0
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК ОбъемБезЗаказа
ИЗ
	(ВЫБРАТЬ
		ВТ_Итог.СерийныйНомер КАК СерийныйНомер,
		МАКСИМУМ(ВТ_Итог.ПериодПоИсторииЗаказа) КАК ПериодПоИсторииЗаказаМакс
	ИЗ
		ВТ_Итог КАК ВТ_Итог
	
	СГРУППИРОВАТЬ ПО
		ВТ_Итог.СерийныйНомер) КАК ВложенныйЗапрос
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Итог КАК ВТ_Итог
			ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
				алк_ОстаткиЗапасов.СерийныйНомер КАК СерийныйНомер,
				МАКСИМУМ(алк_ОстаткиЗапасов.Регистратор) КАК Регистратор
			ИЗ
				РегистрНакопления.алк_ОстаткиЗапасов КАК алк_ОстаткиЗапасов
			ГДЕ
				алк_ОстаткиЗапасов.СерийныйНомер В
						(ВЫБРАТЬ
							ВТ_Итог.СерийныйНомер КАК СерийныйНомер
						ИЗ
							ВТ_Итог КАК ВТ_Итог)
				И алк_ОстаткиЗапасов.Регистратор ССЫЛКА Документ.алк_РасходнаяНакладная
			
			СГРУППИРОВАТЬ ПО
				алк_ОстаткиЗапасов.СерийныйНомер) КАК Расходные
			ПО ВТ_Итог.СерийныйНомер = Расходные.СерийныйНомер
		ПО (ВТ_Итог.ПериодПоИсторииЗаказа = ВложенныйЗапрос.ПериодПоИсторииЗаказаМакс)
			И ВложенныйЗапрос.СерийныйНомер = ВТ_Итог.СерийныйНомер
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ДатаУпаковки КАК ВТ_ДатаУпаковки
		ПО ВложенныйЗапрос.СерийныйНомер = ВТ_ДатаУпаковки.СерийныйНомер

УПОРЯДОЧИТЬ ПО
	ДатаУпаковки</query>
