		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.Регистратор = НЕОПРЕДЕЛЕНО
			ТОГДА NULL
		ИНАЧЕ алк_ОстаткиЗапасовОстаткиИОбороты.Регистратор
	КОНЕЦ КАК Регистратор,
	алк_ОстаткиЗапасовОстаткиИОбороты.ПериодСекунда КАК ПериодСекунда,
	алк_ОстаткиЗапасовОстаткиИОбороты.Организация КАК Организация,
	алк_ОстаткиЗапасовОстаткиИОбороты.СтруктурнаяЕдиница КАК СтруктурнаяЕдиница,
	алк_ОстаткиЗапасовОстаткиИОбороты.Штабель КАК Штабель,
	алк_ОстаткиЗапасовОстаткиИОбороты.Номенклатура КАК Номенклатура,
	алк_ОстаткиЗапасовОстаткиИОбороты.Характеристика КАК Характеристика,
	алк_ОстаткиЗапасовОстаткиИОбороты.Сертификат КАК Сертификат,
	алк_ОстаткиЗапасовОстаткиИОбороты.СерийныйНомер КАК СерийныйНомер,
	алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоНачальныйОстаток КАК КоличествоНачальныйОстаток,
	алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоКонечныйОстаток КАК КоличествоКонечныйОстаток,
	алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоОборот КАК КоличествоОборот,
	алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоПриход КАК КоличествоПриход,
	алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоРасход КАК КоличествоРасход,
	алк_ПараметрыХарактеристик.Порода КАК Порода,
	алк_ПараметрыХарактеристик.Сорт КАК Сорт,
	алк_ПараметрыХарактеристик.Длина КАК Длина,
	алк_ПараметрыХарактеристик.Сечение КАК Сечение,
	алк_ПараметрыХарактеристик.Влажность КАК Влажность,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоНачальныйОстаток
		ИНАЧЕ ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоНачальныйОстаток / алк_ПараметрыХарактеристик.Объем КАК ЧИСЛО(20, 0))
	КОНЕЦ КАК КоличествоСоставНачальныйОстаток,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоКонечныйОстаток
		ИНАЧЕ ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоКонечныйОстаток / алк_ПараметрыХарактеристик.Объем КАК ЧИСЛО(20, 0))
	КОНЕЦ КАК КоличествоСоставКонечныйОстаток,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоОборот
		ИНАЧЕ ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоОборот / алк_ПараметрыХарактеристик.Объем КАК ЧИСЛО(20, 0))
	КОНЕЦ КАК КоличествоСоставОборот,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоПриход
		ИНАЧЕ ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоПриход / алк_ПараметрыХарактеристик.Объем КАК ЧИСЛО(20, 0))
	КОНЕЦ КАК КоличествоСоставПриход,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоРасход
		ИНАЧЕ ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоРасход / алк_ПараметрыХарактеристик.Объем КАК ЧИСЛО(20, 0))
	КОНЕЦ КАК КоличествоСоставРасход,
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоНачальныйОстаток = 0
			ТОГДА 0
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоНачальныйОстаток &lt; 0
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК КоличествоПакетовНачальныйОстаток,
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоКонечныйОстаток = 0
			ТОГДА 0
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоКонечныйОстаток &lt; 0
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК КоличествоПакетовКонечныйОстаток,
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоПриход = 0
			ТОГДА 0
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоПриход &lt; 0
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК КоличествоПакетовПриход,
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоРасход = 0
			ТОГДА 0
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.КоличествоРасход &lt; 0
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК КоличествоПакетовРасход,
	ВЫБОР
		КОГДА ЧАС(алк_ОстаткиЗапасовОстаткиИОбороты.ПериодЧас) МЕЖДУ 8 И 19
			ТОГДА ЗНАЧЕНИЕ(Справочник.алк_Смены.День_2)
		ИНАЧЕ ЗНАЧЕНИЕ(Справочник.алк_Смены.Ночь_2)
	КОНЕЦ КАК Смена,
	ВЫБОР
		КОГДА ЧАС(алк_ОстаткиЗапасовОстаткиИОбороты.ПериодЧас) МЕЖДУ 8 И 23
			ТОГДА НАЧАЛОПЕРИОДА(алк_ОстаткиЗапасовОстаткиИОбороты.ПериодДень, ДЕНЬ)
		ИНАЧЕ ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(алк_ОстаткиЗапасовОстаткиИОбороты.ПериодДень, ДЕНЬ), ДЕНЬ, -1)
	КОНЕЦ КАК ДатаСмены,
	ВЫБОР
		КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.Регистратор ССЫЛКА Документ.алк_Производство
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(алк_ОстаткиЗапасовОстаткиИОбороты.Регистратор КАК Документ.алк_Производство).Операция.Код = "000000008"
						ТОГДА ИСТИНА
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА алк_ОстаткиЗапасовОстаткиИОбороты.Регистратор ССЫЛКА Документ.алк_Перемещение
					ТОГДА ВЫБОР
