	<totalField>
		<dataPath>КоличествоПакетовОборот_Нараст</dataPath>
		<expression>Сумма(КоличествоПакетовОборот_Нараст)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоПакетовОборот_Сутки</dataPath>
