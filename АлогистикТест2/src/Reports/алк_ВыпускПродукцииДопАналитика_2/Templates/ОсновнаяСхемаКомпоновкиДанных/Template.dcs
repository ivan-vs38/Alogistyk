		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Упаковка_Нараст.Организация,
	Упаковка_Нараст.Номенклатура,
	Упаковка_Нараст.Характеристика,
	Упаковка_Нараст.ЗаказПокупателя,
	Упаковка_Нараст.СерийныйНомер,
	Упаковка_Нараст.ДатаСмены,
	Упаковка_Нараст.Смена,
	Упаковка_Нараст.КоличествоПакетовОборот КАК КоличествоПакетовОборот_Нараст,
	Упаковка_Нараст.FSC,
	Упаковка_Нараст.КоличествоПакетовОборот * Упаковка_Нараст.Объем КАК Объем_Нараст,
	Упаковка_Нараст.Порода,
	Упаковка_Нараст.Сорт,
	Упаковка_Нараст.Длина,
	Упаковка_Нараст.Сечение,
	Упаковка_Нараст.Влажность,
	Упаковка_Нараст.КатегорияНоменклатуры,
	ДопАналитика.Аналитика,
	Упаковка_Нараст.БоковаяДоска,
	0 КАК КоличествоПакетовОборот_Сутки,
	0 КАК Объем_Сутки
ИЗ
	(ВЫБРАТЬ
		алк_ВыпускПродукцииСерииОбороты.Организация КАК Организация,
		алк_ВыпускПродукцииСерииОбороты.Номенклатура КАК Номенклатура,
		алк_ВыпускПродукцииСерииОбороты.Характеристика КАК Характеристика,
		алк_ВыпускПродукцииСерииОбороты.ЗаказПокупателя КАК ЗаказПокупателя,
		алк_ВыпускПродукцииСерииОбороты.СерийныйНомер КАК СерийныйНомер,
		алк_ВыпускПродукцииСерииОбороты.ДатаСмены КАК ДатаСмены,
		алк_ВыпускПродукцииСерииОбороты.Смена КАК Смена,
		алк_ВыпускПродукцииСерииОбороты.КоличествоПакетовОборот КАК КоличествоПакетовОборот,
		алк_ПараметрыПакетовСрезПоследних.СертификатFSC КАК FSC,
		алк_ПараметрыПакетовСрезПоследних.Объем КАК Объем,
		ПараметрыХарактеристик.Порода КАК Порода,
		ПараметрыХарактеристик.Сорт КАК Сорт,
		ПараметрыХарактеристик.Длина КАК Длина,
		ПараметрыХарактеристик.Сечение КАК Сечение,
		ПараметрыХарактеристик.Влажность КАК Влажность,
		алк_ВыпускПродукцииСерииОбороты.Номенклатура.КатегорияНоменклатуры КАК КатегорияНоменклатуры,
		алк_ПараметрыПакетовСрезПоследних.БоковаяДоска КАК БоковаяДоска
	ИЗ
		РегистрНакопления.алк_ВыпускПродукцииСерии.Обороты({(&amp;ПустаяДата)}, {(&amp;ПустаяДата)}, Авто, Номенклатура.КатегорияНоменклатуры = ЗНАЧЕНИЕ(Справочник.КатегорииНоменклатуры.Пиломатериал) {(ДатаСмены &gt;= &amp;НачалоПериода
				    И ДатаСмены &lt;= &amp;КонецПериода)}) КАК алк_ВыпускПродукцииСерииОбороты
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыПакетов.СрезПоследних({(&amp;ПериодСреза)}, ) КАК алк_ПараметрыПакетовСрезПоследних
			ПО алк_ВыпускПродукцииСерииОбороты.СерийныйНомер = алк_ПараметрыПакетовСрезПоследних.СерийныйНомер
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК ПараметрыХарактеристик
			ПО алк_ВыпускПродукцииСерииОбороты.Характеристика = ПараметрыХарактеристик.Характеристика) КАК Упаковка_Нараст
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			алк_ДопАналитикаПакетов.СерийныйНомер КАК СерийныйНомер,
			алк_ДопАналитикаПакетов.Аналитика КАК Аналитика
		ИЗ
			РегистрСведений.алк_ДопАналитикаПакетов КАК алк_ДопАналитикаПакетов) КАК ДопАналитика
		ПО Упаковка_Нараст.СерийныйНомер = ДопАналитика.СерийныйНомер

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Упаковка_Сутки.Организация,
	Упаковка_Сутки.Номенклатура,
	Упаковка_Сутки.Характеристика,
	Упаковка_Сутки.ЗаказПокупателя,
	Упаковка_Сутки.СерийныйНомер,
	Упаковка_Сутки.ДатаСмены,
	Упаковка_Сутки.Смена,
	0,
	Упаковка_Сутки.FSC,
	0,
	Упаковка_Сутки.Порода,
	Упаковка_Сутки.Сорт,
	Упаковка_Сутки.Длина,
	Упаковка_Сутки.Сечение,
	Упаковка_Сутки.Влажность,
	Упаковка_Сутки.КатегорияНоменклатуры,
	ДопАналитика.Аналитика,
	Упаковка_Сутки.БоковаяДоска,
	Упаковка_Сутки.КоличествоПакетовОборот,
	Упаковка_Сутки.КоличествоПакетовОборот * Упаковка_Сутки.Объем
ИЗ
	(ВЫБРАТЬ
		алк_ВыпускПродукцииСерииОбороты.Организация КАК Организация,
		алк_ВыпускПродукцииСерииОбороты.Номенклатура КАК Номенклатура,
		алк_ВыпускПродукцииСерииОбороты.Характеристика КАК Характеристика,
		алк_ВыпускПродукцииСерииОбороты.ЗаказПокупателя КАК ЗаказПокупателя,
		алк_ВыпускПродукцииСерииОбороты.СерийныйНомер КАК СерийныйНомер,
		алк_ВыпускПродукцииСерииОбороты.ДатаСмены КАК ДатаСмены,
		алк_ВыпускПродукцииСерииОбороты.Смена КАК Смена,
		алк_ВыпускПродукцииСерииОбороты.КоличествоПакетовОборот КАК КоличествоПакетовОборот,
		алк_ПараметрыПакетовСрезПоследних.СертификатFSC КАК FSC,
		алк_ПараметрыПакетовСрезПоследних.Объем КАК Объем,
		ПараметрыХарактеристик.Порода КАК Порода,
		ПараметрыХарактеристик.Сорт КАК Сорт,
		ПараметрыХарактеристик.Длина КАК Длина,
		ПараметрыХарактеристик.Сечение КАК Сечение,
		ПараметрыХарактеристик.Влажность КАК Влажность,
		алк_ВыпускПродукцииСерииОбороты.Номенклатура.КатегорияНоменклатуры КАК КатегорияНоменклатуры,
		алк_ПараметрыПакетовСрезПоследних.БоковаяДоска КАК БоковаяДоска
	ИЗ
		РегистрНакопления.алк_ВыпускПродукцииСерии.Обороты({(&amp;ПустаяДата)}, {(&amp;ПустаяДата)}, Авто, Номенклатура.КатегорияНоменклатуры = ЗНАЧЕНИЕ(Справочник.КатегорииНоменклатуры.Пиломатериал) {(ДатаСмены = &amp;ДатаСмены)}) КАК алк_ВыпускПродукцииСерииОбороты
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыПакетов.СрезПоследних({(&amp;ПериодСреза)}, ) КАК алк_ПараметрыПакетовСрезПоследних
			ПО алк_ВыпускПродукцииСерииОбороты.СерийныйНомер = алк_ПараметрыПакетовСрезПоследних.СерийныйНомер
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК ПараметрыХарактеристик
			ПО алк_ВыпускПродукцииСерииОбороты.Характеристика = ПараметрыХарактеристик.Характеристика) КАК Упаковка_Сутки
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			алк_ДопАналитикаПакетов.СерийныйНомер КАК СерийныйНомер,
			алк_ДопАналитикаПакетов.Аналитика КАК Аналитика
		ИЗ
			РегистрСведений.алк_ДопАналитикаПакетов КАК алк_ДопАналитикаПакетов) КАК ДопАналитика
		ПО Упаковка_Сутки.СерийныйНомер = ДопАналитика.СерийныйНомер</query>
