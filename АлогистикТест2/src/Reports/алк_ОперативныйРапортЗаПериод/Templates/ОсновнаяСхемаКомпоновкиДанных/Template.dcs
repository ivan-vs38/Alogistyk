				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	алк_ПоказателиОтчетов.Ссылка,
	алк_ПоказателиОтчетов.ПредставлениеПоказателя,
	алк_ПоказателиОтчетов.ЕдиницаИзмерения,
	алк_ПоказателиОтчетов.ВыделятьТекст,
	алк_ПоказателиОтчетов.ВыделятьФон,
	алк_ПоказателиОтчетов.Порядок,
	алк_ПоказателиОтчетов.ТочностьВыводимыхЗначений,
	алк_ПоказателиОтчетов.ОперацияПроизводства,
	алк_ПоказателиОтчетов.Ссылка КАК Показатель
ПОМЕСТИТЬ ВТ_СтруктураОтчета
ИЗ
	Справочник.алк_ПоказателиОтчетов КАК алк_ПоказателиОтчетов
ГДЕ
	алк_ПоказателиОтчетов.Владелец = &amp;Отчет
	И НЕ алк_ПоказателиОтчетов.ЭтоГруппа
	И НЕ алк_ПоказателиОтчетов.ПометкаУдаления

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	алк_ПоказателиОтчетов.Ссылка,
	ВЫБОР
		КОГДА алк_ПоказателиОтчетов.ВыделятьФон
			ТОГДА "Начальные остатки"
		ИНАЧЕ алк_ПоказателиОтчетов.ПредставлениеПоказателя
	КОНЕЦ,
	алк_ПоказателиОтчетов.ЕдиницаИзмерения,
	алк_ПоказателиОтчетов.ВыделятьТекст,
	алк_ПоказателиОтчетов.ВыделятьФон,
	алк_ПоказателиОтчетов.Порядок - 100000,
	алк_ПоказателиОтчетов.ТочностьВыводимыхЗначений,
	алк_ПоказателиОтчетов.ОперацияПроизводства.Наименование + "НаНачало",
	алк_ПоказателиОтчетов.Ссылка
ИЗ
	Справочник.алк_ПоказателиОтчетов КАК алк_ПоказателиОтчетов
ГДЕ
	алк_ПоказателиОтчетов.Владелец = &amp;Отчет
	И НЕ алк_ПоказателиОтчетов.ЭтоГруппа
	И алк_ПоказателиОтчетов.ОперацияПроизводства.ВариантИтогов = &amp;ВариантИтоговПоследнее
	И НЕ алк_ПоказателиОтчетов.ПометкаУдаления
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	алк_ЗначенияПоказателейДляОтчетов.Период,
	алк_ЗначенияПоказателейДляОтчетов.ОперацияПроизводства,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Факт)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеДень
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактДень,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.План)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеДень
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланДень,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Бюджет)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеДень
			ИНАЧЕ 0
		КОНЕЦ) КАК БюджетДень,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Факт)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеНочь
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактНочь,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.План)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеНочь
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланНочь,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Бюджет)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеНочь
			ИНАЧЕ 0
		КОНЕЦ) КАК БюджетНочь,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Факт)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеСутки
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактСутки,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.План)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеСутки
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланСутки,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Бюджет)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеСутки
			ИНАЧЕ 0
		КОНЕЦ) КАК БюджетСутки,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Факт)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеМесяц
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактМесяц,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.План)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеМесяц
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланМесяц,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Бюджет)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеМесяц
			ИНАЧЕ 0
		КОНЕЦ) КАК БюджетМесяц,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Факт)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеГод
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактГод,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.План)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеГод
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланГод,
	СУММА(ВЫБОР
			КОГДА алк_ЗначенияПоказателейДляОтчетов.ВидЗначения = ЗНАЧЕНИЕ(Перечисление.алк_ВидЗначенияПоказателяОтчетов.Бюджет)
				ТОГДА алк_ЗначенияПоказателейДляОтчетов.ЗначениеГод
			ИНАЧЕ 0
		КОНЕЦ) КАК БюджетГод
ПОМЕСТИТЬ ВТ_ДанныеПоказателей
ИЗ
	РегистрСведений.алк_ЗначенияПоказателейДляОтчетов КАК алк_ЗначенияПоказателейДляОтчетов
ГДЕ
	алк_ЗначенияПоказателейДляОтчетов.ОперацияПроизводства В
			(ВЫБРАТЬ
				ВТ.ОперацияПроизводства
			ИЗ
				ВТ_СтруктураОтчета КАК ВТ)
	И алк_ЗначенияПоказателейДляОтчетов.Период МЕЖДУ ДОБАВИТЬКДАТЕ(&amp;НачалоПериодаВыборки, ДЕНЬ, -1) И &amp;ОкончаниеМесяца

СГРУППИРОВАТЬ ПО
	алк_ЗначенияПоказателейДляОтчетов.Период,
	алк_ЗначенияПоказателейДляОтчетов.ОперацияПроизводства
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_СтруктураОтчета.ПредставлениеПоказателя,
	ВТ_СтруктураОтчета.ЕдиницаИзмерения,
	ВТ_СтруктураОтчета.ВыделятьТекст,
	ВТ_СтруктураОтчета.ВыделятьФон,
	ВТ_СтруктураОтчета.Порядок,
	ВЫРАЗИТЬ(ВЗ_Сутки.План КАК ЧИСЛО(15, 0)) КАК СуткиПлан,
	ВЫРАЗИТЬ(ВЗ_Сутки.Факт КАК ЧИСЛО(15, 0)) КАК СуткиФакт,
	ВЫРАЗИТЬ(ВЗ_МесяцНарастающим.План КАК ЧИСЛО(15, 0)) КАК НарастающимПлан,
	ВЫРАЗИТЬ(ВЗ_МесяцНарастающим.Факт КАК ЧИСЛО(15, 0)) КАК НарастающимФакт,
	ВЫРАЗИТЬ(ВЗ_Месяц.Бюджет КАК ЧИСЛО(15, 0)) КАК МесяцБюджет,
	ВЫРАЗИТЬ(ВЗ_Месяц.План КАК ЧИСЛО(15, 0)) КАК МесяцПлан,
	ВЫРАЗИТЬ(ВЗ_Месяц.Факт КАК ЧИСЛО(15, 0)) КАК МесяцФакт,
	ВТ_СтруктураОтчета.Показатель
ИЗ
	ВТ_СтруктураОтчета КАК ВТ_СтруктураОтчета
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВТ_ДанныеПоказателей.ОперацияПроизводства КАК ОперацияПроизводства,
			ВТ_ДанныеПоказателей.ФактСутки КАК Факт,
			ВТ_ДанныеПоказателей.ПланСутки КАК План,
			ВТ_ДанныеПоказателей.БюджетСутки КАК Бюджет
		ИЗ
			ВТ_ДанныеПоказателей КАК ВТ_ДанныеПоказателей
		ГДЕ
			ВТ_ДанныеПоказателей.Период МЕЖДУ &amp;НачалоДня И &amp;ОкончаниеПериода) КАК ВЗ_Сутки
		ПО ВТ_СтруктураОтчета.ОперацияПроизводства = ВЗ_Сутки.ОперацияПроизводства
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			ВТ_ДанныеПоказателей.ОперацияПроизводства КАК ОперацияПроизводства,
			СУММА(ВТ_ДанныеПоказателей.ФактСутки) КАК Факт,
			СУММА(ВТ_ДанныеПоказателей.ПланСутки) КАК План,
			СУММА(ВТ_ДанныеПоказателей.БюджетСутки) КАК Бюджет
		ИЗ
			ВТ_ДанныеПоказателей КАК ВТ_ДанныеПоказателей
		ГДЕ
			ВТ_ДанныеПоказателей.Период МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода
			И ВТ_ДанныеПоказателей.ОперацияПроизводства.ВариантИтогов = &amp;ВариантИтоговСумма
		
		СГРУППИРОВАТЬ ПО
			ВТ_ДанныеПоказателей.ОперацияПроизводства
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ВТ_ДанныеПоказателей.ОперацияПроизводства,
			СРЕДНЕЕ(ВТ_ДанныеПоказателей.ФактСутки),
			СРЕДНЕЕ(ВТ_ДанныеПоказателей.ПланСутки),
			СРЕДНЕЕ(ВТ_ДанныеПоказателей.БюджетСутки)
		ИЗ
			ВТ_ДанныеПоказателей КАК ВТ_ДанныеПоказателей
		ГДЕ
			ВТ_ДанныеПоказателей.Период МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода
			И ВТ_ДанныеПоказателей.ОперацияПроизводства.ВариантИтогов = &amp;ВариантИтоговСреднее
		
		СГРУППИРОВАТЬ ПО
			ВТ_ДанныеПоказателей.ОперацияПроизводства
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ РАЗЛИЧНЫЕ
			ВТ_ДанныеПоказателей.ОперацияПроизводства,
			ВТ_ДанныеПоказателей.ФактМесяц,
			ВТ_ДанныеПоказателей.ПланМесяц,
			ВТ_ДанныеПоказателей.БюджетМесяц
		ИЗ
			ВТ_ДанныеПоказателей КАК ВТ_ДанныеПоказателей
		ГДЕ
			ВТ_ДанныеПоказателей.Период МЕЖДУ &amp;НачалоДня И &amp;ОкончаниеПериода
			И ВТ_ДанныеПоказателей.ОперацияПроизводства.ВариантИтогов = &amp;ВариантИтоговПоследнее) КАК ВЗ_МесяцНарастающим
