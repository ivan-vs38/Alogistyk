		<expression>Сумма(КоличествоОтгрузка)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоЗаказ</dataPath>
		<expression>КоличествоПоАдд</expression>
		<group>Аддендум</group>
	</totalField>
	<totalField>
		<dataPath>КоличествоЗаказ</dataPath>
		<expression>КоличествоВсего</expression>
		<group>ОбщийИтог</group>
	</totalField>
	<totalField>
		<dataPath>КоличествоРезерв</dataPath>
		<expression>Сумма(КоличествоРезерв)</expression>
	</totalField>
