		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_АддендумыОбороты.Аддендум
ПОМЕСТИТЬ ВТ_Аддендумы
ИЗ
	РегистрНакопления.алк_Аддендумы.Обороты(&amp;Дата1 {(&amp;Дата1)}, &amp;Дата2 {(&amp;Дата2)}, , ) КАК алк_АддендумыОбороты
ГДЕ
	(алк_АддендумыОбороты.Аддендум = &amp;Аддендум
			ИЛИ ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка) = &amp;Аддендум)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СУММА(алк_АддендумыТовары.Количество) КАК Количество
ПОМЕСТИТЬ ВТ_ЗаказаноВсего
ИЗ
	Документ.алк_Аддендумы.Товары КАК алк_АддендумыТовары
ГДЕ
	алк_АддендумыТовары.Ссылка В
			(ВЫБРАТЬ
				ВТ_Аддендумы.Аддендум
			ИЗ
				ВТ_Аддендумы КАК ВТ_Аддендумы)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_АддендумыТовары.Ссылка,
	СУММА(алк_АддендумыТовары.Количество) КАК Количество
ПОМЕСТИТЬ ВТ_ЗаказаноПоАдд
ИЗ
	Документ.алк_Аддендумы.Товары КАК алк_АддендумыТовары
ГДЕ
	алк_АддендумыТовары.Ссылка В
			(ВЫБРАТЬ
				ВТ_Аддендумы.Аддендум
			ИЗ
				ВТ_Аддендумы КАК ВТ_Аддендумы)

СГРУППИРОВАТЬ ПО
	алк_АддендумыТовары.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_АддендумыТовары.Ссылка,
	алк_АддендумыТовары.ИдентификаторТовара,
	алк_АддендумыТовары.ПредставлениеТовара,
	алк_АддендумыТовары.Номенклатура,
	алк_АддендумыТовары.Количество,
	алк_АддендумыТовары.Цена,
	алк_АддендумыТовары.Сумма,
	ВТ_ЗаказаноПоАдд.Количество КАК КоличествоПоАдд,
	ВТ_ЗаказаноВсего.Количество КАК КоличествоВсего
ПОМЕСТИТЬ ВТ_Заказано
ИЗ
	Документ.алк_Аддендумы.Товары КАК алк_АддендумыТовары
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ЗаказаноПоАдд КАК ВТ_ЗаказаноПоАдд
		ПО алк_АддендумыТовары.Ссылка = ВТ_ЗаказаноПоАдд.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ЗаказаноВсего КАК ВТ_ЗаказаноВсего
		ПО (ИСТИНА)
ГДЕ
	алк_АддендумыТовары.Ссылка В
			(ВЫБРАТЬ
				ВТ_Аддендумы.Аддендум
			ИЗ
				ВТ_Аддендумы КАК ВТ_Аддендумы)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ПакетыПоАддендумамСрезПоследних.СерийныйНомер,
	алк_ПакетыПоАддендумамСрезПоследних.ИдентификаторТовара,
	алк_ПакетыПоАддендумамСрезПоследних.Количество,
	алк_ПакетыПоАддендумамСрезПоследних.Регистратор,
	алк_ПараметрыХарактеристик.Порода,
	алк_ПараметрыХарактеристик.Сорт,
	алк_ПараметрыХарактеристик.Длина,
	алк_ПараметрыХарактеристик.Сечение,
	алк_ПараметрыХарактеристик.Влажность
ПОМЕСТИТЬ ВТ_Отгрузка
ИЗ
	РегистрСведений.алк_ПакетыПоАддендумам.СрезПоследних(
			,
			Аддендум В
					(ВЫБРАТЬ
						ВТ_Аддендумы.Аддендум
					ИЗ
						ВТ_Аддендумы КАК ВТ_Аддендумы)
				И ПоОтгрузке) КАК алк_ПакетыПоАддендумамСрезПоследних
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
		ПО алк_ПакетыПоАддендумамСрезПоследних.Характеристика = алк_ПараметрыХарактеристик.Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ТранспортировкаЗапасовЗапасы.СерийныйНомер,
	алк_ТранспортировкаЗапасовЗапасы.Ссылка.СтруктурнаяЕдиницаПолучатель,
	алк_ТранспортировкаЗапасовЗапасы.Ссылка КАК Транспортировка
ПОМЕСТИТЬ ВТ_Транспортировки
ИЗ
	Документ.алк_ТранспортировкаЗапасов.Запасы КАК алк_ТранспортировкаЗапасовЗапасы
ГДЕ
	алк_ТранспортировкаЗапасовЗапасы.Ссылка.Проведен
	И алк_ТранспортировкаЗапасовЗапасы.СерийныйНомер В
			(ВЫБРАТЬ
				ВТ_Отгрузка.СерийныйНомер
			ИЗ
				ВТ_Отгрузка КАК ВТ_Отгрузка)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ПакетыПоАддендумамСрезПоследних.СерийныйНомер,
	алк_ПакетыПоАддендумамСрезПоследних.ИдентификаторТовара,
	алк_ПакетыПоАддендумамСрезПоследних.Количество,
	алк_ПакетыПоАддендумамСрезПоследних.Регистратор,
	алк_ПараметрыХарактеристик.Порода,
	алк_ПараметрыХарактеристик.Сорт,
	алк_ПараметрыХарактеристик.Длина,
	алк_ПараметрыХарактеристик.Сечение,
	алк_ПараметрыХарактеристик.Влажность
ПОМЕСТИТЬ ВТ_Резерв
ИЗ
	РегистрСведений.алк_ПакетыПоАддендумам.СрезПоследних(
			,
			Аддендум В
					(ВЫБРАТЬ
						ВТ.Аддендум
					ИЗ
						ВТ_Аддендумы КАК ВТ)
				И ПоОтгрузке = ЛОЖЬ) КАК алк_ПакетыПоАддендумамСрезПоследних
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
		ПО алк_ПакетыПоАддендумамСрезПоследних.Характеристика = алк_ПараметрыХарактеристик.Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Заказано.Ссылка КАК Аддендум,
	ВТ_Заказано.ИдентификаторТовара,
	ВТ_Заказано.ПредставлениеТовара,
	ВТ_Заказано.Номенклатура,
	ВТ_Заказано.Количество КАК КоличествоЗаказ,
	ВТ_Заказано.Цена,
	ВТ_Заказано.Сумма,
	ВТ_Отгрузка.СерийныйНомер,
	ВТ_Отгрузка.Количество КАК КоличествоОтгрузка,
	ВТ_Заказано.КоличествоПоАдд,
	ВТ_Заказано.КоличествоВсего,
	0 КАК КоличествоРезерв,
	ВТ_Отгрузка.Порода,
	ВТ_Отгрузка.Сорт,
	ВТ_Отгрузка.Длина,
	ВТ_Отгрузка.Сечение,
	ВТ_Отгрузка.Влажность,
	ВТ_Отгрузка.Регистратор КАК ДокументОтгрузки,
	NULL КАК ДокументРезерв,
	ВТ_Транспортировки.СтруктурнаяЕдиницаПолучатель КАК Порт,
	ВТ_Транспортировки.Транспортировка
ИЗ
	ВТ_Заказано КАК ВТ_Заказано
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Отгрузка КАК ВТ_Отгрузка
			ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Транспортировки КАК ВТ_Транспортировки
			ПО ВТ_Отгрузка.СерийныйНомер = ВТ_Транспортировки.СерийныйНомер
		ПО ВТ_Заказано.ИдентификаторТовара = ВТ_Отгрузка.ИдентификаторТовара

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_Заказано.Ссылка,
	ВТ_Заказано.ИдентификаторТовара,
	ВТ_Заказано.ПредставлениеТовара,
	ВТ_Заказано.Номенклатура,
	ВТ_Заказано.Количество,
	ВТ_Заказано.Цена,
	ВТ_Заказано.Сумма,
	ВТ_Резерв.СерийныйНомер,
	0,
	ВТ_Заказано.КоличествоПоАдд,
	ВТ_Заказано.КоличествоВсего,
	ВТ_Резерв.Количество,
	ВТ_Резерв.Порода,
	ВТ_Резерв.Сорт,
	ВТ_Резерв.Длина,
	ВТ_Резерв.Сечение,
	ВТ_Резерв.Влажность,
	NULL,
	ВТ_Резерв.Регистратор,
	NULL,
	NULL
ИЗ
	ВТ_Заказано КАК ВТ_Заказано
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Резерв КАК ВТ_Резерв
		ПО ВТ_Заказано.ИдентификаторТовара = ВТ_Резерв.ИдентификаторТовара</query>
	</dataSet>
	<totalField>
		<dataPath>КоличествоЗаказ</dataPath>
		<expression>Максимум(КоличествоЗаказ)</expression>
		<group>ДокументОтгрузки</group>
		<group>ДокументРезерв</group>
		<group>ПредставлениеТовара</group>
		<group>Транспортировка</group>
	</totalField>
	<totalField>
