		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	алк_ЗапасыНаСкладахСерииОстатки.Организация,
	алк_ЗапасыНаСкладахСерииОстатки.СтруктурнаяЕдиница,
	алк_ЗапасыНаСкладахСерииОстатки.Ячейка,
	алк_ЗапасыНаСкладахСерииОстатки.СерийныйНомер,
	алк_ПараметрыПакетовСрезПоследних.НомерПакета,
	алк_ПараметрыПакетовСрезПоследних.ПериодПакета,
	алк_ПараметрыПакетовСрезПоследних.КоличествоСостав,
	алк_ПараметрыПакетовСрезПоследних.Объем * алк_ЗапасыНаСкладахСерииОстатки.КоличествоПакетовОстаток КАК ОбъемПакетовОстаток,
	алк_ПараметрыПакетовСрезПоследних.СертификатFSC,
	алк_ЗапасыНаСкладахСерииОстатки.Номенклатура,
	алк_ПараметрыПакетовСрезПоследних.Характеристика,
	ПараметрыХарактеристик.Порода,
	ПараметрыХарактеристик.Сорт,
	ПараметрыХарактеристик.Длина,
	ПараметрыХарактеристик.Сечение,
	ПараметрыХарактеристик.Влажность,
	алк_ЗапасыНаСкладахСерииОстатки.Номенклатура.КатегорияНоменклатуры КАК КатегорияНоменклатуры,
	алк_ЗапасыНаСкладахСерииОстатки.КоличествоПакетовОстаток
ИЗ
	РегистрНакопления.алк_ЗапасыНаСкладахСерии.Остатки(
			{(&amp;НаДату)},
			СтруктурнаяЕдиница = &amp;СтруктурнаяЕдиница
				И Номенклатура.КатегорияНоменклатуры В (&amp;П1)) КАК алк_ЗапасыНаСкладахСерииОстатки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыПакетов.СрезПоследних({(&amp;ПериодСреза)}, ) КАК алк_ПараметрыПакетовСрезПоследних
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК ПараметрыХарактеристик
			ПО алк_ПараметрыПакетовСрезПоследних.Характеристика = ПараметрыХарактеристик.Характеристика
		ПО алк_ЗапасыНаСкладахСерииОстатки.СерийныйНомер = алк_ПараметрыПакетовСрезПоследних.СерийныйНомер

УПОРЯДОЧИТЬ ПО
	ПараметрыХарактеристик.Порода,
	ПараметрыХарактеристик.Длина.ДлинаММ,
	ПараметрыХарактеристик.Сечение.ТолщинаММ,
	ПараметрыХарактеристик.Сечение.ШиринаММ</query>
