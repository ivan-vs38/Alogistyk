	Для Инд = 0 По СтрокаДанных.КодВидОпер.Количество() - 1 Цикл
		КомпоновщикXML_СоздатьУзел(КомпоновщикXML, "КодВидОпер");
		КомпоновщикXML_ВывестиЗначениеИЗакрытьУзел(КомпоновщикXML, СтрокаДанных.КодВидОпер[Инд]);
