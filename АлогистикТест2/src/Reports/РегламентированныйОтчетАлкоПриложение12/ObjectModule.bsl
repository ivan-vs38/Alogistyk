			КонецЕсли;
			
			ИмяГрафы = ПрефиксИмени + "10";
			ИмяПоказателя = ПрефиксИмени + "10_" + НомерСтрокиСтроковый;
			ЗначениеГрафы = СокрЛП(СтрокаДекларации[ИмяГрафы]);
