		ПО ВТ.ЗаказПокупателя = алк_ЗаказПокупателяОбъемныеГруппы.Ссылка
			И (алк_ЗаказПокупателяОбъемныеГруппы.стрНомераКатегорияПЭО ПОДОБНО "%" + ВТ.КатегорияНоменклатурыПЭО.Код + "%")

