	ВТ_Оперативный.ДопАналитика,
	ВТ_Оперативный.Сертификат,
	ВТ_Оперативный.Габарит,
	ВТ_Оперативный.КатегорияНоменклатурыПЭО,
	ВТ_Оперативный.Припуск,
	ВТ_Оперативный.ДатаСмены,
	ВТ_Оперативный.Смена,
	ВТ_Оперативный.Ответственный,
	ВТ_Оперативный.КоличествоПакетов,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыПакетовСрезПоследних.СерийныйНомер, 0) = 0
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Производство
ИЗ
	ВТ_Оперативный КАК ВТ_Оперативный
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыПакетов.СрезПоследних(
				&amp;Дата3 {(&amp;Дата3)},
				СерийныйНомер В
					(ВЫБРАТЬ
						ВТ_Оперативный.СерийныйНомер
					ИЗ
						ВТ_Оперативный КАК ВТ_Оперативный)) КАК алк_ПараметрыПакетовСрезПоследних
		ПО ВТ_Оперативный.СерийныйНомер = алк_ПараметрыПакетовСрезПоследних.СерийныйНомер

