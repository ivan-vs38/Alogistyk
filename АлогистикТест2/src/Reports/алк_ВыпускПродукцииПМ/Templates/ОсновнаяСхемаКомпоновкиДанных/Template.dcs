	алк_ПараметрыХарактеристик.Сорт,
	алк_ПараметрыХарактеристик.Длина,
	алк_ПараметрыХарактеристик.Сечение,
	алк_ПараметрыХарактеристик.Влажность,
	ВЫБОР
		КОГДА Упаковка.СерийныйНомер = ЗНАЧЕНИЕ(Справочник.СерийныеНомера.ПустаяСсылка)
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ,
	ВЫБОР
		КОГДА ЕСТЬNULL(алк_ПараметрыХарактеристик.Объем, 0) = 0
			ТОГДА 0
		ИНАЧЕ ОКР(Упаковка.Объем / алк_ПараметрыХарактеристик.Объем)
	КОНЕЦ,
	Упаковка.Объем,
	ВЫБОР
		КОГДА Упаковка.КоличествоПакетов = ЗНАЧЕНИЕ(Справочник.СерийныеНомера.ПустаяСсылка)
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ,
	NULL,
	NULL,
	NULL,
	МАКСИМУМ(Упаковка.Регистратор),
	НАЧАЛОПЕРИОДА(Упаковка.Период, МЕСЯЦ)
ИЗ
	Упаковка КАК Упаковка
