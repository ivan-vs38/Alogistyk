		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	РаспиленоЗаПериод.Участок КАК Участок,
	РаспиленоЗаПериод.Номенклатура КАК Номенклатура,
	РаспиленоЗаПериод.Характеристика КАК Характеристика,
	РаспиленоЗаПериод.Объем КАК Объем,
	РаспиленоЗаПериод.Регистратор КАК Регистратор,
	РаспиленоЗаПериод.Период КАК Период,
	РаспиленоЗаПериод.Сертификат КАК Сертификат,
	РаспиленоЗаПериод.Направление КАК Направление,
	РаспиленоЗаПериод.Смена КАК Смена
ПОМЕСТИТЬ ЗатратыМатериаловНаВыпуск_СтарыйМеханизм
ИЗ
	(ВЫБРАТЬ
		алк_СборкаЗапасовЗапасы.Ссылка.Участок КАК Участок,
		алк_СборкаЗапасовЗапасы.Номенклатура КАК Номенклатура,
		алк_СборкаЗапасовЗапасы.Характеристика КАК Характеристика,
		алк_СборкаЗапасовЗапасы.Объем КАК Объем,
		алк_СборкаЗапасовЗапасы.Ссылка КАК Регистратор,
		алк_СборкаЗапасовЗапасы.Ссылка.ДатаСмены КАК Период,
		алк_СборкаЗапасовЗапасы.Сертификат КАК Сертификат,
		ВЫБОР
			КОГДА алк_СборкаЗапасовЗапасы.Ссылка.Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление)
				ТОГДА "МЛП"
			ИНАЧЕ "ЦЛП"
		КОНЕЦ КАК Направление,
		ВЫБОР
			КОГДА алк_СборкаЗапасовЗапасы.Ссылка.Смена = ЗНАЧЕНИЕ(Справочник.алк_Смены.Ночь_3)
				ТОГДА ЗНАЧЕНИЕ(Справочник.алк_Смены.Ночь_2)
			ИНАЧЕ алк_СборкаЗапасовЗапасы.Ссылка.Смена
		КОНЕЦ КАК Смена
	ИЗ
		Документ.алк_СборкаЗапасов.Запасы КАК алк_СборкаЗапасовЗапасы
	ГДЕ
		алк_СборкаЗапасовЗапасы.Ссылка.Проведен
		И алк_СборкаЗапасовЗапасы.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
		И алк_СборкаЗапасовЗапасы.Номенклатура.Код = "ПБ-00000002"
		И (алк_СборкаЗапасовЗапасы.Ссылка.Дата &lt; ДАТАВРЕМЯ(2024, 12, 18)
					И алк_СборкаЗапасовЗапасы.Ссылка.Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.Лесопиление)
				ИЛИ алк_СборкаЗапасовЗапасы.Ссылка.Дата &lt; ДАТАВРЕМЯ(2025, 4, 14)
					И алк_СборкаЗапасовЗапасы.Ссылка.Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление))
		И НЕ &amp;Упакованное) КАК РаспиленоЗаПериод
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВЫБОР
		КОГДА алк_СменныйРапортЦЛПТ_Выход.Ссылка.Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление)
			ТОГДА "МЛП"
		ИНАЧЕ "ЦЛП"
	КОНЕЦ КАК Направление,
	СУММА(алк_СменныйРапортЦЛПТ_Выход.Объем) КАК ОбъемВыходРасчет,
	СУММА(алк_СменныйРапортЦЛПТ_Выход.Количество) КАК КолВыходРасчет,
	алк_СменныйРапортЦЛПТ_Выход.Ссылка КАК Регистратор
ПОМЕСТИТЬ РасчетВыходИРаспил
ИЗ
	Документ.алк_СменныйРапортЦЛП.Т_Выход КАК алк_СменныйРапортЦЛПТ_Выход
ГДЕ
	алк_СменныйРапортЦЛПТ_Выход.Ссылка.Проведен
	И алк_СменныйРапортЦЛПТ_Выход.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И НЕ &amp;Упакованное

СГРУППИРОВАТЬ ПО
	алк_СменныйРапортЦЛПТ_Выход.Ссылка,
	ВЫБОР
		КОГДА алк_СменныйРапортЦЛПТ_Выход.Ссылка.Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление)
			ТОГДА "МЛП"
		ИНАЧЕ "ЦЛП"
	КОНЕЦ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ПроизводствоОборотОбороты.ДатаСмены КАК ДатаСмены,
	алк_ПроизводствоОборотОбороты.Смена КАК Смена,
	алк_ПроизводствоОборотОбороты.Участок КАК Участок,
	алк_ПроизводствоОборотОбороты.Номенклатура КАК Номенклатура,
	алк_ПроизводствоОборотОбороты.Характеристика КАК Характеристика,
	алк_ПроизводствоОборотОбороты.КоличествоОборот КАК Объем,
	алк_ПроизводствоОборотОбороты.Регистратор КАК Регистратор,
	алк_ПроизводствоОборотОбороты.Сертификат КАК Сертификат,
	ВЫБОР
		КОГДА алк_ПроизводствоОборотОбороты.Регистратор ССЫЛКА Документ.алк_СборкаЗапасов
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(алк_ПроизводствоОборотОбороты.Регистратор КАК Документ.алк_СборкаЗапасов).Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление)
						ТОГДА "МЛП"
					ИНАЧЕ "ЦЛП"
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА алк_ПроизводствоОборотОбороты.Участок.Код = "000000044"
					ТОГДА "МЛП"
				ИНАЧЕ "ЦЛП"
			КОНЕЦ
	КОНЕЦ КАК Направление
ПОМЕСТИТЬ ЗатратыМатериаловНаВыпуск
ИЗ
	РегистрНакопления.алк_ПроизводствоОборот.Обороты(
			{(&amp;НачалоПериода)},
			{(&amp;КонецПериода)},
			Регистратор,
			ВидДвиженияПроизводства = ЗНАЧЕНИЕ(Перечисление.алк_ВидыДвиженияПроизводствоОборот.Списано)
				И Операция В (ЗНАЧЕНИЕ(Справочник.алк_ОперацииПроизводства.Распил), ЗНАЧЕНИЕ(Справочник.алк_ОперацииПроизводства.Упаковка))
				И Номенклатура.Код = "ПБ-00000002") КАК алк_ПроизводствоОборотОбороты
ГДЕ
	НЕ алк_ПроизводствоОборотОбороты.Регистратор В
				(ВЫБРАТЬ
					ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Регистратор КАК Регистратор
				ИЗ
					ЗатратыМатериаловНаВыпуск_СтарыйМеханизм КАК ЗатратыМатериаловНаВыпуск_СтарыйМеханизм)
	И НЕ &amp;Упакованное

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Период,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Смена,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Участок,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Номенклатура,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Характеристика,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Объем,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Регистратор,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Сертификат,
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм.Направление
ИЗ
	ЗатратыМатериаловНаВыпуск_СтарыйМеханизм КАК ЗатратыМатериаловНаВыпуск_СтарыйМеханизм
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ПроизводствоОборотОбороты.Участок КАК Участок,
	алк_ПроизводствоОборотОбороты.СерийныйНомер КАК СерийныйНомер,
	алк_ПроизводствоОборотОбороты.Номенклатура КАК Номенклатура,
	алк_ПроизводствоОборотОбороты.Характеристика КАК Характеристика,
	алк_ПроизводствоОборотОбороты.СерийныйНомер КАК КоличествоПакетов,
	СУММА(алк_ПроизводствоОборотОбороты.КоличествоОборот) КАК Объем,
	алк_ПроизводствоОборотОбороты.Регистратор КАК Регистратор,
	алк_ПроизводствоОборотОбороты.ДатаСмены КАК ДатаСмены,
	алк_ПроизводствоОборотОбороты.Смена КАК Смена,
	алк_ПроизводствоОборотОбороты.ДатаСмены КАК Период,
	алк_ПроизводствоОборотОбороты.Сертификат КАК Сертификат,
	ВЫБОР
		КОГДА алк_ПроизводствоОборотОбороты.Регистратор ССЫЛКА Документ.алк_СборкаЗапасов
			ТОГДА ВЫБОР
					КОГДА ВЫРАЗИТЬ(алк_ПроизводствоОборотОбороты.Регистратор КАК Документ.алк_СборкаЗапасов).Направление = ЗНАЧЕНИЕ(Перечисление.алк_НаправленияДеятельности.МалоеЛесопиление)
						ТОГДА "МЛП"
					ИНАЧЕ "ЦЛП"
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА алк_ПроизводствоОборотОбороты.Участок.Код = "000000044"
					ТОГДА "МЛП"
				ИНАЧЕ "ЦЛП"
			КОНЕЦ
	КОНЕЦ КАК Направление
ПОМЕСТИТЬ Упаковка_ПоНовымРегистрам
ИЗ
	РегистрНакопления.алк_ПроизводствоОборот.Обороты(
			{(&amp;НачалоПериода)},
			{(&amp;КонецПериода)},
			Регистратор,
			ВидДвиженияПроизводства = ЗНАЧЕНИЕ(Перечисление.алк_ВидыДвиженияПроизводствоОборот.Оприходовано)
