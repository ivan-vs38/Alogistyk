;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ЕСТЬNULL(ТекущийПериод.ДатаСмены, ИнвентаризацияПрошлого.НачалоПрошлогоПериода) КАК ДатаСмены,
	ТекущийПериод.Смена КАК Смена,
	ТекущийПериод.НоменклатураР КАК НоменклатураР,
	ЕСТЬNULL(ТекущийПериод.ХарактеристикаР, ИнвентаризацияПрошлого.ХарактеристикаСырье) КАК ХарактеристикаР,
	МАКСИМУМ(ТекущийПериод.КоличествоР) КАК КоличествоР,
	МАКСИМУМ(ТекущийПериод.ОбъемР) КАК ОбъемР,
	ТекущийПериод.НоменклатураВ КАК НоменклатураВ,
	ТекущийПериод.ХарактеристикаВ КАК ХарактеристикаВ,
	СУММА(ТекущийПериод.КоличествоВ) КАК КоличествоВ,
	СУММА(ТекущийПериод.ОбъемВ) КАК ОбъемВ,
	ТекущийПериод.Инвентаризация КАК Инвентаризация,
	ИнвентаризацияПрошлого.ОбъемПродукция КАК ИнвентаризацияПрошлого,
	ТекущийПериод.Направление КАК Направление,
	СУММА(ТекущийПериод.ОбъемВТ) КАК ОбъемВТ
ПОМЕСТИТЬ Вт_Данные
ИЗ
	(ВЫБРАТЬ РАЗЛИЧНЫЕ
		ЕСТЬNULL(Продукция.ДатаСмены, Сырье.ДатаСмены) КАК ДатаСмены,
		ЕСТЬNULL(Продукция.Смена, Сырье.Смена) КАК Смена,
		ЕСТЬNULL(Продукция.Номенклатура, Сырье.Номенклатура) КАК НоменклатураР,
		Сырье.Характеристика КАК ХарактеристикаР,
		МАКСИМУМ(ЕСТЬNULL(Сырье.Количество, 0)) КАК КоличествоР,
		МАКСИМУМ(ЕСТЬNULL(Сырье.Объем, 0)) КАК ОбъемР,
		Продукция.Номенклатура КАК НоменклатураВ,
		Продукция.Характеристика КАК ХарактеристикаВ,
		СУММА(Продукция.Количество) КАК КоличествоВ,
		СУММА(Продукция.Объем) КАК ОбъемВ,
		ВЫБОР
			КОГДА Продукция.Аналитика_3 = "Инвентаризация"
				ТОГДА Продукция.Объем
			ИНАЧЕ 0
		КОНЕЦ КАК Инвентаризация,
		Продукция.Направление КАК Направление,
		Продукция.ОбъемТ КАК ОбъемВТ
	ИЗ
		(ВЫБРАТЬ
			алк_СменныйРапорт.Регистратор.Ссылка КАК РегистраторСсылка,
			алк_СменныйРапорт.Смена КАК Смена,
			алк_СменныйРапорт.ДатаСмены КАК ДатаСмены,
			алк_СменныйРапорт.Номенклатура КАК Номенклатура,
			алк_СменныйРапорт.Характеристика КАК Характеристика,
			МИНИМУМ(алк_СменныйРапорт.Аналитика_1) КАК Аналитика_1,
			алк_СменныйРапорт.Аналитика_2 КАК Аналитика_2,
			алк_СменныйРапорт.Аналитика_3 КАК Аналитика_3,
			СУММА(алк_СменныйРапорт.Количество) КАК Количество,
			СУММА(алк_СменныйРапорт.Объем) КАК Объем,
			алк_СменныйРапорт.Направление КАК Направление
		ИЗ
			РегистрНакопления.алк_СменныйРапорт КАК алк_СменныйРапорт
		ГДЕ
			алк_СменныйРапорт.Показатель = ЗНАЧЕНИЕ(Справочник.алк_ПоказателиРапортов.ТРаспилено)
			И алк_СменныйРапорт.ДатаСмены МЕЖДУ &amp;ДатаСмены1 И &amp;ДатаСмены2
		
		СГРУППИРОВАТЬ ПО
			алк_СменныйРапорт.Регистратор.Ссылка,
			алк_СменныйРапорт.Аналитика_2,
			алк_СменныйРапорт.Аналитика_3,
			алк_СменныйРапорт.ДатаСмены,
			алк_СменныйРапорт.Номенклатура,
			алк_СменныйРапорт.Смена,
			алк_СменныйРапорт.Характеристика,
			алк_СменныйРапорт.Направление) КАК Сырье
			ПОЛНОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
				алк_СменныйРапорт.Регистратор.Ссылка КАК РегистраторСсылка,
				алк_СменныйРапорт.Смена КАК Смена,
				алк_СменныйРапорт.ДатаСмены КАК ДатаСмены,
				алк_СменныйРапорт.Номенклатура КАК Номенклатура,
				алк_СменныйРапорт.Характеристика КАК Характеристика,
				алк_СменныйРапорт.Аналитика_1 КАК Аналитика_1,
				алк_СменныйРапорт.Аналитика_2 КАК Аналитика_2,
				алк_СменныйРапорт.Аналитика_3 КАК Аналитика_3,
				СУММА(алк_СменныйРапорт.Количество) КАК Количество,
				СУММА(ВЫБОР
						КОГДА алк_СменныйРапорт.Аналитика_3 = "ВыходСУчетомПрипуска"
							ТОГДА 0
						ИНАЧЕ алк_СменныйРапорт.Объем
					КОНЕЦ) КАК Объем,
				алк_СменныйРапорт.Направление КАК Направление,
				ВЫБОР
					КОГДА алк_СменныйРапорт.Аналитика_3 = "ВыходСУчетомПрипуска"
						ТОГДА алк_СменныйРапорт.Объем
					ИНАЧЕ 0
				КОНЕЦ КАК ОбъемТ
			ИЗ
				РегистрНакопления.алк_СменныйРапорт КАК алк_СменныйРапорт
			ГДЕ
				алк_СменныйРапорт.Показатель = ЗНАЧЕНИЕ(Справочник.алк_ПоказателиРапортов.ТВыход)
				И алк_СменныйРапорт.ДатаСмены МЕЖДУ &amp;ДатаСмены1 И &amp;ДатаСмены2
			
			СГРУППИРОВАТЬ ПО
				алк_СменныйРапорт.Номенклатура,
				алк_СменныйРапорт.Регистратор.Ссылка,
				алк_СменныйРапорт.Смена,
				алк_СменныйРапорт.ДатаСмены,
				алк_СменныйРапорт.Характеристика,
				алк_СменныйРапорт.Аналитика_2,
				алк_СменныйРапорт.Аналитика_3,
				алк_СменныйРапорт.Направление,
				алк_СменныйРапорт.Аналитика_1,
				ВЫБОР
					КОГДА алк_СменныйРапорт.Аналитика_3 = "ВыходСУчетомПрипуска"
						ТОГДА алк_СменныйРапорт.Объем
					ИНАЧЕ 0
				КОНЕЦ) КАК Продукция
			ПО Сырье.РегистраторСсылка = Продукция.РегистраторСсылка
				И Сырье.Аналитика_1 = Продукция.Аналитика_1
				И Сырье.Направление = Продукция.Направление
	
	СГРУППИРОВАТЬ ПО
		ЕСТЬNULL(Продукция.Смена, Сырье.Смена),
		ЕСТЬNULL(Продукция.Номенклатура, Сырье.Номенклатура),
		Сырье.Характеристика,
		Продукция.Номенклатура,
		Продукция.Характеристика,
		ВЫБОР
			КОГДА Продукция.Аналитика_3 = "Инвентаризация"
				ТОГДА Продукция.Объем
			ИНАЧЕ 0
		КОНЕЦ,
		ЕСТЬNULL(Продукция.ДатаСмены, Сырье.ДатаСмены),
		Продукция.Направление,
		Продукция.ОбъемТ) КАК ТекущийПериод
		ПОЛНОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
			СырьеИнвПрошлого.НачалоПрошлогоПериода КАК НачалоПрошлогоПериода,
			СырьеИнвПрошлого.Номенклатура КАК НоменклатураСырье,
			СырьеИнвПрошлого.Характеристика КАК ХарактеристикаСырье,
			СырьеИнвПрошлого.Количество КАК КоличествоСырье,
			СырьеИнвПрошлого.Объем КАК ОбъемСырье,
			ПродукцияИнвПрошлого.Количество КАК КоличествоПродукция,
			ПродукцияИнвПрошлого.Объем КАК ОбъемПродукция,
			ПродукцияИнвПрошлого.Направление КАК Направление
		ИЗ
			(ВЫБРАТЬ РАЗЛИЧНЫЕ
				алк_СменныйРапорт.Регистратор КАК Регистратор,
				алк_СменныйРапорт.Номенклатура КАК Номенклатура,
				алк_СменныйРапорт.Характеристика КАК Характеристика,
				алк_СменныйРапорт.Аналитика_1 КАК Аналитика_1,
				алк_СменныйРапорт.Аналитика_2 КАК Аналитика_2,
				алк_СменныйРапорт.Аналитика_3 КАК Аналитика_3,
				алк_СменныйРапорт.Количество КАК Количество,
				алк_СменныйРапорт.Объем КАК Объем,
				алк_СменныйРапорт.Направление КАК Направление
			ИЗ
				РегистрНакопления.алк_СменныйРапорт КАК алк_СменныйРапорт
			ГДЕ
				алк_СменныйРапорт.ДатаСмены МЕЖДУ ДОБАВИТЬКДАТЕ(&amp;ДатаСмены1, МЕСЯЦ, -1) И ДОБАВИТЬКДАТЕ(&amp;ДатаСмены1, СЕКУНДА, -1)
				И алк_СменныйРапорт.Показатель = ЗНАЧЕНИЕ(Справочник.алк_ПоказателиРапортов.ТВыход)
				И алк_СменныйРапорт.Аналитика_3 = "Инвентаризация") КАК ПродукцияИнвПрошлого
				ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ РАЗЛИЧНЫЕ
					алк_СменныйРапорт.Регистратор КАК Регистратор,
					алк_СменныйРапорт.Номенклатура КАК Номенклатура,
					алк_СменныйРапорт.Характеристика КАК Характеристика,
					алк_СменныйРапорт.Аналитика_1 КАК Аналитика_1,
					алк_СменныйРапорт.Аналитика_2 КАК Аналитика_2,
					алк_СменныйРапорт.Аналитика_3 КАК Аналитика_3,
					алк_СменныйРапорт.Количество КАК Количество,
					алк_СменныйРапорт.Объем КАК Объем,
					ДОБАВИТЬКДАТЕ(&amp;ДатаСмены1, МЕСЯЦ, -1) КАК НачалоПрошлогоПериода,
					алк_СменныйРапорт.Направление КАК Направление
				ИЗ
					РегистрНакопления.алк_СменныйРапорт КАК алк_СменныйРапорт
				ГДЕ
					алк_СменныйРапорт.ДатаСмены МЕЖДУ ДОБАВИТЬКДАТЕ(&amp;ДатаСмены1, МЕСЯЦ, -1) И ДОБАВИТЬКДАТЕ(&amp;ДатаСмены1, СЕКУНДА, -1)
					И алк_СменныйРапорт.Показатель = ЗНАЧЕНИЕ(Справочник.алк_ПоказателиРапортов.ТРаспилено)
					И алк_СменныйРапорт.Аналитика_3 = "Инвентаризация") КАК СырьеИнвПрошлого
				ПО ПродукцияИнвПрошлого.Регистратор = СырьеИнвПрошлого.Регистратор
					И ПродукцияИнвПрошлого.Аналитика_1 = СырьеИнвПрошлого.Аналитика_1
					И ПродукцияИнвПрошлого.Направление = СырьеИнвПрошлого.Направление) КАК ИнвентаризацияПрошлого
		ПО ТекущийПериод.ХарактеристикаР = ИнвентаризацияПрошлого.ХарактеристикаСырье
			И ТекущийПериод.ХарактеристикаР = ИнвентаризацияПрошлого.Направление

СГРУППИРОВАТЬ ПО
	ИнвентаризацияПрошлого.ОбъемПродукция,
	ЕСТЬNULL(ТекущийПериод.ДатаСмены, ИнвентаризацияПрошлого.НачалоПрошлогоПериода),
	ТекущийПериод.Смена,
	ТекущийПериод.НоменклатураР,
	ТекущийПериод.НоменклатураВ,
	ТекущийПериод.ХарактеристикаВ,
	ТекущийПериод.Инвентаризация,
	ЕСТЬNULL(ТекущийПериод.ХарактеристикаР, ИнвентаризацияПрошлого.ХарактеристикаСырье),
	ТекущийПериод.Направление
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Вт_Данные.ДатаСмены,
	Вт_Данные.Смена,
	Вт_Данные.НоменклатураР,
	Вт_Данные.ХарактеристикаР,
	Вт_Данные.КоличествоР,
	Вт_Данные.ОбъемР
ПОМЕСТИТЬ ВТ_РазличноеРаспиленное
ИЗ
	Вт_Данные КАК Вт_Данные
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена,
	МАКСИМУМ(ВТ_РазличноеРаспиленное.ОбъемР) КАК ОбъемР
ПОМЕСТИТЬ ВТ_МаксимальныйОбъемРаспиленного
ИЗ
	ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное

СГРУППИРОВАТЬ ПО
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	алк_СменныйРапортЦЛП.ДатаСмены,
	алк_СменныйРапортЦЛП.Смена,
	алк_СменныйРапортЦЛП.СбросыМеталл,
	алк_СменныйРапортЦЛП.СбросыНесоответсвия,
	алк_СменныйРапортЦЛП.Направление,
	НАЧАЛОПЕРИОДА(алк_СменныйРапортЦЛП.ДатаСмены, МЕСЯЦ) КАК Месяц
ПОМЕСТИТЬ ВТ_Сбросы
ИЗ
	Документ.алк_СменныйРапортЦЛП КАК алк_СменныйРапортЦЛП
ГДЕ
	алк_СменныйРапортЦЛП.Проведен
	И алк_СменныйРапортЦЛП.ДатаСмены МЕЖДУ &amp;ДатаСмены1 И &amp;ДатаСмены2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВсеСмены.ДатаСмены КАК ДатаСмены,
	ВсеСмены.Смена,
	СУММА(ЕСТЬNULL(алк_УчетПростоевОбороты.ВремяПростояОборот, 0)) КАК ВремяПростоя
ПОМЕСТИТЬ ВТ_ОбщееВремяПростоя
ИЗ
	(ВЫБРАТЬ
		ДатыСмен.Период КАК ДатаСмены,
		ЗНАЧЕНИЕ(Справочник.алк_Смены.День_2) КАК Смена
	ИЗ
		ДатыСмен КАК ДатыСмен
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ДатыСмен.Период,
		ЗНАЧЕНИЕ(Справочник.алк_Смены.Ночь_2)
	ИЗ
		ДатыСмен КАК ДатыСмен) КАК ВсеСмены
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.алк_УчетПростоев.Обороты(
				,
				,
				,
				Дата МЕЖДУ &amp;ДатаСмены1 И &amp;ДатаСмены2
					И Участок.Код = "000000002") КАК алк_УчетПростоевОбороты
		ПО ВсеСмены.ДатаСмены = алк_УчетПростоевОбороты.Дата
			И ВсеСмены.Смена = алк_УчетПростоевОбороты.Смена

СГРУППИРОВАТЬ ПО
	ВсеСмены.ДатаСмены,
	ВсеСмены.Смена
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена,
	"Подано в производство, м3" КАК Показатель,
	СУММА(ВЫРАЗИТЬ(ВТ_РазличноеРаспиленное.ОбъемР КАК ЧИСЛО(15, 3))) КАК Значение
ПОМЕСТИТЬ ВТ_Показатели
ИЗ
	ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное

СГРУППИРОВАТЬ ПО
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена,
	"Подано в производство, шт.",
	СУММА(ВТ_РазличноеРаспиленное.КоличествоР)
ИЗ
	ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное

СГРУППИРОВАТЬ ПО
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_ОбщееВремяПростоя.ДатаСмены,
	ВТ_ОбщееВремяПростоя.Смена,
	"Чистое время работы, час",
	СУММА(ВЫБОР
			КОГДА ОКР(12 - ВТ_ОбщееВремяПростоя.ВремяПростоя / 60 / 60, 1) = 0
				ТОГДА NULL
			ИНАЧЕ ОКР(12 - ВТ_ОбщееВремяПростоя.ВремяПростоя / 60 / 60, 1)
		КОНЕЦ)
ИЗ
	ВТ_ОбщееВремяПростоя КАК ВТ_ОбщееВремяПростоя

СГРУППИРОВАТЬ ПО
	ВТ_ОбщееВремяПростоя.ДатаСмены,
	ВТ_ОбщееВремяПростоя.Смена

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена,
	"Производительность, м3/час",
	СУММА(ВЫБОР
			КОГДА 12 - ЕСТЬNULL(ВТ_ОбщееВремяПростоя.ВремяПростоя, 0) / 60 / 60 &lt;&gt; 0
				ТОГДА ВЫРАЗИТЬ(ВТ_РазличноеРаспиленное.ОбъемР / (12 - ЕСТЬNULL(ВТ_ОбщееВремяПростоя.ВремяПростоя, 0) / 60 / 60) КАК ЧИСЛО(15, 1))
			ИНАЧЕ ВЫРАЗИТЬ(ВТ_РазличноеРаспиленное.ОбъемР КАК ЧИСЛО(15, 1))
		КОНЕЦ)
ИЗ
	ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ОбщееВремяПростоя КАК ВТ_ОбщееВремяПростоя
		ПО ВТ_РазличноеРаспиленное.ДатаСмены = ВТ_ОбщееВремяПростоя.ДатаСмены
			И ВТ_РазличноеРаспиленное.Смена = ВТ_ОбщееВремяПростоя.Смена

СГРУППИРОВАТЬ ПО
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена,
	"Производительность, шт./мин",
	СУММА(ВЫБОР
			КОГДА 12 * 60 - ЕСТЬNULL(ВТ_ОбщееВремяПростоя.ВремяПростоя, 0) / 60 &lt;&gt; 0
				ТОГДА ОКР(ВТ_РазличноеРаспиленное.КоличествоР / (12 * 60 - ЕСТЬNULL(ВТ_ОбщееВремяПростоя.ВремяПростоя, 0) / 60))
			ИНАЧЕ ОКР(ВТ_РазличноеРаспиленное.КоличествоР)
		КОНЕЦ)
ИЗ
	ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ОбщееВремяПростоя КАК ВТ_ОбщееВремяПростоя
		ПО ВТ_РазличноеРаспиленное.ДатаСмены = ВТ_ОбщееВремяПростоя.ДатаСмены
			И ВТ_РазличноеРаспиленное.Смена = ВТ_ОбщееВремяПростоя.Смена

СГРУППИРОВАТЬ ПО
	ВТ_РазличноеРаспиленное.ДатаСмены,
	ВТ_РазличноеРаспиленное.Смена

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_МаксимальныйОбъемРаспиленного.ДатаСмены,
	ВТ_МаксимальныйОбъемРаспиленного.Смена,
	"Ср.диаметр, см",
	СУММА(ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ДиаметрММ, 0) / 10)
ИЗ
	ВТ_МаксимальныйОбъемРаспиленного КАК ВТ_МаксимальныйОбъемРаспиленного
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_РазличноеРаспиленное КАК ВТ_РазличноеРаспиленное
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
			ПО ВТ_РазличноеРаспиленное.ХарактеристикаР = алк_ПараметрыХарактеристик.Характеристика
		ПО ВТ_МаксимальныйОбъемРаспиленного.ДатаСмены = ВТ_РазличноеРаспиленное.ДатаСмены
			И ВТ_МаксимальныйОбъемРаспиленного.Смена = ВТ_РазличноеРаспиленное.Смена
			И ВТ_МаксимальныйОбъемРаспиленного.ОбъемР = ВТ_РазличноеРаспиленное.ОбъемР

СГРУППИРОВАТЬ ПО
