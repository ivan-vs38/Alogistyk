    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_ОстаткиЗапасовОстатки.СерийныйНомер КАК Ссылка,
	СУММА(алк_ОстаткиЗапасовОстатки.КоличествоОстаток) КАК Количество,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Порода, ЗНАЧЕНИЕ(Справочник.Породы.ПустаяСсылка)) КАК Порода,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сорт, ЗНАЧЕНИЕ(Справочник.Сорта.ПустаяСсылка)) КАК Сорт,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Длина.ДлинаММ, 0) КАК Длина,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ШиринаММ, 0) КАК Ширина,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Влажность.ЗначениеВлажности, 100) КАК Влажность,
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ТолщинаММ, 0) КАК Толщина,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.Припуск, ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)) КАК Припуск,
	алк_ОстаткиЗапасовОстатки.Характеристика,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.КатегорияНоменклатурыПЭО, ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)) КАК КатегорияНоменклатурыПЭО,
	алк_ОстаткиЗапасовОстатки.Номенклатура,
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Аддендум, ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)) КАК АддендумВыбор,
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Период, ДАТАВРЕМЯ(1, 1, 1)) КАК ПериодАдд
ПОМЕСТИТЬ ВТ_СН
ИЗ
	РегистрНакопления.алк_ОстаткиЗапасов.Остатки({(&amp;ДатаОстатков)}, {((&amp;ВсеОрганизации
			    ИЛИ Организация = &amp;Организация)
			    И (&amp;ВсеСтруктурныеЕдиницы
			        ИЛИ СтруктурнаяЕдиница = &amp;СтруктурнаяЕдиница)
			    И (&amp;ВсеШтабеля
			        ИЛИ Штабель = &amp;Штабель))}) КАК алк_ОстаткиЗапасовОстатки
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СерийныеНомера КАК СправочникСерийныеНомера
		ПО алк_ОстаткиЗапасовОстатки.СерийныйНомер = СправочникСерийныеНомера.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
		ПО алк_ОстаткиЗапасовОстатки.Характеристика = алк_ПараметрыХарактеристик.Характеристика
			И (&amp;ОтборПоАддендуму)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ДополнительныеПараметрыПакетов.СрезПоследних КАК алк_ДополнительныеПараметрыПакетовСрезПоследних
		ПО (&amp;ОтборПоАддендуму)
			И алк_ОстаткиЗапасовОстатки.СерийныйНомер = алк_ДополнительныеПараметрыПакетовСрезПоследних.СерийныйНомер
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПакетыПоАддендумам.СрезПоследних КАК алк_ПакетыПоАддендумамСрезПоследних
		ПО алк_ОстаткиЗапасовОстатки.СерийныйНомер = алк_ПакетыПоАддендумамСрезПоследних.СерийныйНомер
			И (&amp;ДобавитьОтборПоАддендуму)
ГДЕ
	&amp;ТолькоПакетыНаОстатках

СГРУППИРОВАТЬ ПО
	алк_ОстаткиЗапасовОстатки.СерийныйНомер,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.Припуск, ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Влажность.ЗначениеВлажности, 100),
	алк_ОстаткиЗапасовОстатки.Характеристика,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.КатегорияНоменклатурыПЭО, ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Порода, ЗНАЧЕНИЕ(Справочник.Породы.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сорт, ЗНАЧЕНИЕ(Справочник.Сорта.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ШиринаММ, 0),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Длина.ДлинаММ, 0),
	ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ТолщинаММ, 0),
	алк_ОстаткиЗапасовОстатки.Номенклатура,
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Аддендум, ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Период, ДАТАВРЕМЯ(1, 1, 1))
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	алк_АддендумыСоставТоваров.ИдентификаторТовара КАК ИдентификаторТовара,
	алк_АддендумыСоставТоваров.Порода КАК Порода,
	алк_АддендумыСоставТоваров.Сорт КАК Сорт,
	алк_АддендумыСоставТоваров.ШиринаОт КАК ШиринаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ШиринаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ШиринаДо
	КОНЕЦ КАК ШиринаДо,
	алк_АддендумыСоставТоваров.ТолщинаОт КАК ТолщинаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ТолщинаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ТолщинаДо
	КОНЕЦ КАК ТолщинаДо,
	алк_АддендумыСоставТоваров.ДлинаОт КАК ДлинаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ДлинаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ДлинаДо
	КОНЕЦ КАК ДлинаДо,
	алк_АддендумыСоставТоваров.ВлажностьОт КАК ВлажностьОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ВлажностьДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ВлажностьДо
	КОНЕЦ КАК ВлажностьДо,
	алк_АддендумыСоставТоваров.ВариантПрипуска КАК ВариантПрипуска,
	алк_АддендумыТовары.КатегорияПЭО КАК КатегорияНоменклатурыПЭО,
	алк_АддендумыСоставТоваров.Характеристика,
	алк_АддендумыТовары.Номенклатура
ПОМЕСТИТЬ ВТ_СоставАддендума
ИЗ
	Документ.алк_Аддендумы.СоставТоваров КАК алк_АддендумыСоставТоваров
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.алк_Аддендумы.Товары КАК алк_АддендумыТовары
		ПО алк_АддендумыСоставТоваров.Ссылка = алк_АддендумыТовары.Ссылка
			И алк_АддендумыСоставТоваров.ИдентификаторТовара = алк_АддендумыТовары.ИдентификаторТовара
ГДЕ
	алк_АддендумыСоставТоваров.Ссылка = &amp;Аддендум
	И &amp;ОтборПоАддендуму
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	СерийныеНомера.Ссылка КАК Ссылка,
	СерийныеНомера.ВерсияДанных КАК ВерсияДанных,
	СерийныеНомера.ПометкаУдаления КАК ПометкаУдаления,
	СерийныеНомера.Владелец КАК Владелец,
	СерийныеНомера.Наименование КАК Наименование,
	СерийныеНомера.Продан КАК Продан,
	СерийныеНомера.НомерКиЗГИСМ КАК НомерКиЗГИСМ,
	СерийныеНомера.RFIDTID КАК RFIDTID,
	СерийныеНомера.RFIDUser КАК RFIDUser,
	СерийныеНомера.RFIDEPC КАК RFIDEPC,
	СерийныеНомера.EPCGTIN КАК EPCGTIN,
	СерийныеНомера.НомерГИСМ КАК НомерГИСМ,
	СерийныеНомера.Предопределенный КАК Предопределенный,
	СерийныеНомера.ИмяПредопределенныхДанных КАК ИмяПредопределенныхДанных,
	ВТ_СН.Количество КАК Количество,
	ВТ_СН.Характеристика КАК Характеристика,
	ВТ_СН.АддендумВыбор
ПОМЕСТИТЬ ВТ_СН_ПоПоследнемуАдд
ИЗ
	(ВЫБРАТЬ
		ВТ_СН.Ссылка КАК Ссылка,
		МАКСИМУМ(ВТ_СН.ПериодАдд) КАК ПериодАддМакс
	ИЗ
		ВТ_СН КАК ВТ_СН
	
	СГРУППИРОВАТЬ ПО
		ВТ_СН.Ссылка) КАК ВложенныйЗапрос
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СН КАК ВТ_СН
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СерийныеНомера КАК СерийныеНомера
			ПО ВТ_СН.Ссылка = СерийныеНомера.Ссылка
		ПО ВложенныйЗапрос.Ссылка = ВТ_СН.Ссылка
			И ВложенныйЗапрос.ПериодАддМакс = ВТ_СН.ПериодАдд
ГДЕ
	&amp;ДобавитьОтборПоАддендуму
	И &amp;ТолькоПакетыНаОстатках
	И НЕ ВТ_СН.Ссылка ЕСТЬ NULL
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	СерийныеНомера.Ссылка,
	СерийныеНомера.ВерсияДанных,
	СерийныеНомера.ПометкаУдаления,
	СерийныеНомера.Владелец,
	СерийныеНомера.Наименование,
	СерийныеНомера.Продан,
	СерийныеНомера.НомерКиЗГИСМ,
	СерийныеНомера.RFIDTID,
	СерийныеНомера.RFIDUser,
	СерийныеНомера.RFIDEPC,
	СерийныеНомера.EPCGTIN,
	СерийныеНомера.НомерГИСМ,
	СерийныеНомера.Предопределенный,
	СерийныеНомера.ИмяПредопределенныхДанных,
	ВТ_СН.Количество,
	ВТ_СН.Характеристика,
	NULL КАК Аддендум
ПОМЕСТИТЬ ВТ
ИЗ
	ВТ_СН КАК ВТ_СН
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СерийныеНомера КАК СерийныеНомера
		ПО ВТ_СН.Ссылка = СерийныеНомера.Ссылка
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_СоставАддендума КАК ВТ_СоставАддендума
		ПО (ВТ_СН.Порода = ВТ_СоставАддендума.Порода
				ИЛИ ВТ_СоставАддендума.Порода = ЗНАЧЕНИЕ(Справочник.Породы.ПустаяСсылка))
			И (ВТ_СН.Сорт = ВТ_СоставАддендума.Сорт
				ИЛИ ВТ_СоставАддендума.Сорт = ЗНАЧЕНИЕ(Справочник.Сорта.ПустаяСсылка))
			И ВТ_СН.Длина >= ВТ_СоставАддендума.ДлинаОт
			И ВТ_СН.Длина &lt;= ВТ_СоставАддендума.ДлинаДо
			И ВТ_СН.Ширина >= ВТ_СоставАддендума.ШиринаОт
			И ВТ_СН.Ширина &lt;= ВТ_СоставАддендума.ШиринаДо
			И ВТ_СН.Влажность >= ВТ_СоставАддендума.ВлажностьОт
			И ВТ_СН.Влажность &lt;= ВТ_СоставАддендума.ВлажностьДо
			И ВТ_СН.Толщина >= ВТ_СоставАддендума.ТолщинаОт
			И ВТ_СН.Толщина &lt;= ВТ_СоставАддендума.ТолщинаДо
			И (ВТ_СН.Припуск = ВТ_СоставАддендума.ВариантПрипуска
				ИЛИ ВТ_СоставАддендума.ВариантПрипуска = ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)
				ИЛИ ИСТИНА)
			И (ВТ_СН.КатегорияНоменклатурыПЭО = ВТ_СоставАддендума.КатегорияНоменклатурыПЭО
				ИЛИ ВТ_СоставАддендума.КатегорияНоменклатурыПЭО = ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)
				ИЛИ НЕ &amp;ОтборПоКатегорииПЭО)
			И (ВТ_СН.Характеристика = ВТ_СоставАддендума.Характеристика
				ИЛИ ВТ_СоставАддендума.Характеристика = ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка))
			И ВТ_СН.Номенклатура = ВТ_СоставАддендума.Номенклатура
ГДЕ
	&amp;ОтборПоАддендуму
	И &amp;ТолькоПакетыНаОстатках

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ РАЗЛИЧНЫЕ
	СерийныеНомера.Ссылка,
	СерийныеНомера.ВерсияДанных,
	СерийныеНомера.ПометкаУдаления,
	СерийныеНомера.Владелец,
	СерийныеНомера.Наименование,
	СерийныеНомера.Продан,
	СерийныеНомера.НомерКиЗГИСМ,
	СерийныеНомера.RFIDTID,
	СерийныеНомера.RFIDUser,
	СерийныеНомера.RFIDEPC,
	СерийныеНомера.EPCGTIN,
	СерийныеНомера.НомерГИСМ,
	СерийныеНомера.Предопределенный,
	СерийныеНомера.ИмяПредопределенныхДанных,
	ВТ_СН.Количество,
	ВТ_СН.Характеристика,
	NULL
ИЗ
	ВТ_СН КАК ВТ_СН
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СерийныеНомера КАК СерийныеНомера
		ПО ВТ_СН.Ссылка = СерийныеНомера.Ссылка
ГДЕ
	НЕ &amp;ОтборПоАддендуму
	И &amp;ТолькоПакетыНаОстатках
	И НЕ &amp;ДобавитьОтборПоАддендуму

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СправочникСерийныеНомера.Ссылка,
	СправочникСерийныеНомера.ВерсияДанных,
	СправочникСерийныеНомера.ПометкаУдаления,
	СправочникСерийныеНомера.Владелец,
	СправочникСерийныеНомера.Наименование,
	СправочникСерийныеНомера.Продан,
	СправочникСерийныеНомера.НомерКиЗГИСМ,
	СправочникСерийныеНомера.RFIDTID,
	СправочникСерийныеНомера.RFIDUser,
	СправочникСерийныеНомера.RFIDEPC,
	СправочникСерийныеНомера.EPCGTIN,
	СправочникСерийныеНомера.НомерГИСМ,
	СправочникСерийныеНомера.Предопределенный,
	СправочникСерийныеНомера.ИмяПредопределенныхДанных,
	0,
	NULL,
	NULL
ИЗ
	Справочник.СерийныеНомера КАК СправочникСерийныеНомера
ГДЕ
	ВЫБОР
			КОГДА &amp;ПоказатьПроданные
				ТОГДА ИСТИНА
			ИНАЧЕ НЕ СправочникСерийныеНомера.Продан
		КОНЕЦ
	И НЕ &amp;ТолькоПакетыНаОстатках
	И НЕ &amp;ОтборПоАддендуму

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВТ_СН_ПоПоследнемуАдд.Ссылка,
	ВТ_СН_ПоПоследнемуАдд.ВерсияДанных,
	ВТ_СН_ПоПоследнемуАдд.ПометкаУдаления,
	ВТ_СН_ПоПоследнемуАдд.Владелец,
	ВТ_СН_ПоПоследнемуАдд.Наименование,
	ВТ_СН_ПоПоследнемуАдд.Продан,
	ВТ_СН_ПоПоследнемуАдд.НомерКиЗГИСМ,
	ВТ_СН_ПоПоследнемуАдд.RFIDTID,
	ВТ_СН_ПоПоследнемуАдд.RFIDUser,
	ВТ_СН_ПоПоследнемуАдд.RFIDEPC,
	ВТ_СН_ПоПоследнемуАдд.EPCGTIN,
