    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	алк_АддендумыСоставТоваров.ИдентификаторТовара КАК ИдентификаторТовара,
	алк_АддендумыСоставТоваров.ШиринаОт КАК ШиринаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ШиринаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ШиринаДо
	КОНЕЦ КАК ШиринаДо,
	алк_АддендумыСоставТоваров.ТолщинаОт КАК ТолщинаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ТолщинаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ТолщинаДо
	КОНЕЦ КАК ТолщинаДо,
	алк_АддендумыСоставТоваров.ДлинаОт КАК ДлинаОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ДлинаДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ДлинаДо
	КОНЕЦ КАК ДлинаДо,
	алк_АддендумыСоставТоваров.Порода КАК Порода,
	алк_АддендумыСоставТоваров.Сорт КАК Сорт,
	алк_АддендумыСоставТоваров.ВариантПрипуска КАК Припуск,
	алк_АддендумыТовары.КатегорияПЭО КАК КатегорияНоменклатурыПЭО,
	алк_АддендумыСоставТоваров.ВлажностьОт КАК ВлажностьОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ВлажностьДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ВлажностьДо
	КОНЕЦ КАК ВлажностьДо,
	алк_АддендумыСоставТоваров.Характеристика,
	алк_АддендумыТовары.Номенклатура,
	алк_АддендумыСоставТоваров.ДиаметрОт,
	ВЫБОР
		КОГДА алк_АддендумыСоставТоваров.ДиаметрДо = 0
			ТОГДА 999999
		ИНАЧЕ алк_АддендумыСоставТоваров.ДиаметрДо
	КОНЕЦ КАК ДиаметрДо
ПОМЕСТИТЬ втСостав
ИЗ
	Документ.алк_Аддендумы.СоставТоваров КАК алк_АддендумыСоставТоваров
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.алк_Аддендумы.Товары КАК алк_АддендумыТовары
		ПО алк_АддендумыСоставТоваров.Ссылка = алк_АддендумыТовары.Ссылка
			И алк_АддендумыСоставТоваров.ИдентификаторТовара = алк_АддендумыТовары.ИдентификаторТовара
ГДЕ
	алк_АддендумыСоставТоваров.Ссылка = &amp;Аддендум
	И алк_АддендумыСоставТоваров.ИдентификаторТовара = &amp;ИдентификаторТовара
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	алк_ПараметрыХарактеристик.Характеристика КАК Характеристика,
	втСостав.ИдентификаторТовара,
	втСостав.КатегорияНоменклатурыПЭО КАК КатегорияНоменклатурыПЭО,
	втСостав.Припуск КАК Припуск,
	втСостав.Номенклатура
ПОМЕСТИТЬ втСостСХаракт
ИЗ
	втСостав КАК втСостав
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
		ПО (втСостав.Порода = ЕСТЬNULL(алк_ПараметрыХарактеристик.Порода, ЗНАЧЕНИЕ(Справочник.Породы.ПустаяСсылка))
				ИЛИ втСостав.Порода = ЗНАЧЕНИЕ(Справочник.Породы.ПустаяСсылка))
			И (втСостав.Сорт = ЕСТЬNULL(алк_ПараметрыХарактеристик.Сорт, ЗНАЧЕНИЕ(Справочник.Сорта.ПустаяСсылка))
				ИЛИ втСостав.Сорт = ЗНАЧЕНИЕ(Справочник.Сорта.ПустаяСсылка))
			И (втСостав.ШиринаОт &lt;= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ШиринаММ, 0))
			И (втСостав.ШиринаДо >= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ШиринаММ, 0))
			И (втСостав.ТолщинаОт &lt;= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ТолщинаММ, 0))
			И (втСостав.ТолщинаДо >= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ТолщинаММ, 0))
			И (втСостав.ДлинаОт &lt;= ЕСТЬNULL(алк_ПараметрыХарактеристик.Длина.ДлинаММ, 0))
			И (втСостав.ДлинаДо >= ЕСТЬNULL(алк_ПараметрыХарактеристик.Длина.ДлинаММ, 0))
			И (втСостав.ВлажностьОт &lt;= ЕСТЬNULL(алк_ПараметрыХарактеристик.Влажность.ЗначениеВлажности, 0))
			И (втСостав.ВлажностьДо >= ЕСТЬNULL(алк_ПараметрыХарактеристик.Влажность.ЗначениеВлажности, 0))
			И (втСостав.Характеристика = алк_ПараметрыХарактеристик.Характеристика
				ИЛИ втСостав.Характеристика = ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка))
			И (втСостав.ДиаметрОт &lt;= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ДиаметрММ, 0))
			И (втСостав.ДиаметрДо >= ЕСТЬNULL(алк_ПараметрыХарактеристик.Сечение.ДиаметрММ, 0))
ГДЕ
	(алк_ПараметрыХарактеристик.Порода В (&amp;Порода)
			ИЛИ &amp;ВсеПорода)
	И (алк_ПараметрыХарактеристик.Сорт В (&amp;Сорт)
			ИЛИ &amp;ВсеСорт)
	И (алк_ПараметрыХарактеристик.Длина В (&amp;Длина)
			ИЛИ &amp;ВсеДлина)
	И (алк_ПараметрыХарактеристик.Сечение В (&amp;Сечение)
			ИЛИ &amp;ВсеСечение)
	И (алк_ПараметрыХарактеристик.Влажность В (&amp;Влажность)
			ИЛИ &amp;ВсеВлажность)

СГРУППИРОВАТЬ ПО
	алк_ПараметрыХарактеристик.Характеристика,
	втСостав.ИдентификаторТовара,
	втСостав.КатегорияНоменклатурыПЭО,
	втСостав.Припуск,
	втСостав.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	алк_ОстаткиЗапасовОстатки.СерийныйНомер,
	алк_ОстаткиЗапасовОстатки.Характеристика,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.КатегорияНоменклатурыПЭО, ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)) КАК КатегорияНоменклатурыПЭО,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.Припуск, ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)) КАК Припуск,
	СУММА(алк_ОстаткиЗапасовОстатки.КоличествоОстаток) КАК Объем,
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Аддендум, ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)) КАК Аддендум,
	алк_ОстаткиЗапасовОстатки.Номенклатура
ПОМЕСТИТЬ ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску
ИЗ
	РегистрНакопления.алк_ОстаткиЗапасов.Остатки(
			,
			Характеристика В
					(ВЫБРАТЬ
						втСостСХаракт.Характеристика
					ИЗ
						втСостСХаракт КАК втСостСХаракт)
				И НЕ СерийныйНомер В
						(ВЫБРАТЬ
							алк_ПакетыПоАддендумамСрезПоследних.СерийныйНомер
						ИЗ
							РегистрСведений.алк_ПакетыПоАддендумам.СрезПоследних(, Аддендум = &amp;Аддендум
								И НЕ ПоОтгрузке) КАК алк_ПакетыПоАддендумамСрезПоследних)
				И СтруктурнаяЕдиница.алк_ТипСклада В (ЗНАЧЕНИЕ(Перечисление.алк_ТипСклада.СГП), ЗНАЧЕНИЕ(Перечисление.алк_ТипСклада.Производство))) КАК алк_ОстаткиЗапасовОстатки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ДополнительныеПараметрыПакетов.СрезПоследних КАК алк_ДополнительныеПараметрыПакетовСрезПоследних
		ПО алк_ОстаткиЗапасовОстатки.СерийныйНомер = алк_ДополнительныеПараметрыПакетовСрезПоследних.СерийныйНомер
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПакетыПоАддендумам.СрезПоследних(, НЕ ПоОтгрузке) КАК алк_ПакетыПоАддендумамСрезПоследних
		ПО алк_ОстаткиЗапасовОстатки.СерийныйНомер = алк_ПакетыПоАддендумамСрезПоследних.СерийныйНомер

СГРУППИРОВАТЬ ПО
	алк_ОстаткиЗапасовОстатки.Характеристика,
	алк_ОстаткиЗапасовОстатки.СерийныйНомер,
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.КатегорияНоменклатурыПЭО, ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)),
	ЕСТЬNULL(алк_ДополнительныеПараметрыПакетовСрезПоследних.Припуск, ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)),
	ЕСТЬNULL(алк_ПакетыПоАддендумамСрезПоследних.Аддендум, ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)),
	алк_ОстаткиЗапасовОстатки.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Объем) КАК Объем,
	втСостСХаракт.ИдентификаторТовара,
	СерийныеНомера.Ссылка КАК СерийныйНомер,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Аддендум КАК ТекущийАддендум,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Характеристика,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.КатегорияНоменклатурыПЭО,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Припуск,
	&quot;     &quot; КАК КолонкаВыравнивания
ИЗ
	втСостСХаракт КАК втСостСХаракт
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску КАК ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.СерийныеНомера КАК СерийныеНомера
			ПО ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.СерийныйНомер = СерийныеНомера.Ссылка
		ПО (ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Характеристика = втСостСХаракт.Характеристика)
			И (ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.КатегорияНоменклатурыПЭО = втСостСХаракт.КатегорияНоменклатурыПЭО
				ИЛИ втСостСХаракт.КатегорияНоменклатурыПЭО = ЗНАЧЕНИЕ(Справочник.алк_КатегорияНоменклатурыПЭО.ПустаяСсылка)
				ИЛИ НЕ &amp;УчитыватьКатегориюПЭО)
			И (ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Припуск = втСостСХаракт.Припуск
				ИЛИ втСостСХаракт.Припуск = ЗНАЧЕНИЕ(Справочник.алк_ВариантыПрипуска.ПустаяСсылка)
				ИЛИ НЕ &amp;УчитыватьПрипуск)
			И втСостСХаракт.Номенклатура = ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Номенклатура
ГДЕ
	(&amp;ТолькоСвободные
				И ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Аддендум = ЗНАЧЕНИЕ(Документ.алк_Аддендумы.ПустаяСсылка)
			ИЛИ НЕ &amp;ТолькоСвободные)

СГРУППИРОВАТЬ ПО
	втСостСХаракт.ИдентификаторТовара,
	СерийныеНомера.Ссылка,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Характеристика,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.КатегорияНоменклатурыПЭО,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Припуск,
	ВТ_ОстаткиПоХаракКатегорииПЭОПрипуску.Аддендум</queryText>
      <mainTable>Catalog.СерийныеНомера</mainTable>
