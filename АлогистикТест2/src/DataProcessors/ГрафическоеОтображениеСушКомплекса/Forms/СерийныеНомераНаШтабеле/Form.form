    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	алк_ОстаткиЗапасовОстатки.СерийныйНомер,
	алк_ОстаткиЗапасовОстатки.Характеристика,
	алк_ОстаткиЗапасовОстатки.КоличествоОстаток КАК Объем,
	ОКР(алк_ОстаткиЗапасовОстатки.КоличествоОстаток / алк_ПараметрыХарактеристик.Объем) КАК КоличествоСостав,
	ВложенныйЗапрос.КоличествоСоставИтог,
	ВложенныйЗапрос.ОбъемИтог
ИЗ
	РегистрНакопления.алк_ОстаткиЗапасов.Остатки(
			,
			Номенклатура.КатегорияНоменклатуры = &amp;Номенклатура
				И СтруктурнаяЕдиница = &amp;Склад
				И Штабель = &amp;Штабель) КАК алк_ОстаткиЗапасовОстатки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
		ПО алк_ОстаткиЗапасовОстатки.Характеристика = алк_ПараметрыХарактеристик.Характеристика,
	(ВЫБРАТЬ
		СУММА(алк_ОстаткиЗапасовОстатки.КоличествоОстаток) КАК ОбъемИтог,
		алк_ОстаткиЗапасовОстатки.Штабель КАК Штабель,
		СУММА(ОКР(алк_ОстаткиЗапасовОстатки.КоличествоОстаток / алк_ПараметрыХарактеристик.Объем)) КАК КоличествоСоставИтог
	ИЗ
		РегистрНакопления.алк_ОстаткиЗапасов.Остатки(
				,
				Номенклатура.КатегорияНоменклатуры = &amp;Номенклатура
					И СтруктурнаяЕдиница = &amp;Склад
					И Штабель = &amp;Штабель) КАК алк_ОстаткиЗапасовОстатки
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыХарактеристик КАК алк_ПараметрыХарактеристик
			ПО алк_ОстаткиЗапасовОстатки.Характеристика = алк_ПараметрыХарактеристик.Характеристика
	
	СГРУППИРОВАТЬ ПО
		алк_ОстаткиЗапасовОстатки.Штабель) КАК ВложенныйЗапрос</queryText>
      <mainTable>AccumulationRegister.алк_ОстаткиЗапасов.Balance</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
