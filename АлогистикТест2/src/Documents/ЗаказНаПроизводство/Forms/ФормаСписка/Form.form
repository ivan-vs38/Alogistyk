  <attributes>
    <name>Список</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА НЕ ДокументЗаказНаПроизводство.Проведен
				ИЛИ ДокументЗаказНаПроизводство.ВариантЗавершения = ЗНАЧЕНИЕ(Перечисление.ВариантыЗавершенияЗаказа.Отменен)
			ТОГДА 2
		КОГДА ЕСТЬNULL(ЗаказыНаПроизводствоОстаткиИОбороты.КоличествоПриход, 0) = 0
				И ЕСТЬNULL(ЗаказыНаПроизводствоОстаткиИОбороты.КоличествоРасход, 0) = 0
			ТОГДА ВЫБОР
					КОГДА ДокументЗаказНаПроизводство.СостояниеЗаказа = ЗНАЧЕНИЕ(Справочник.СостоянияЗаказовНаПроизводство.Завершен)
						ТОГДА 2
					КОГДА ДокументЗаказНаПроизводство.Финиш &lt; &amp;АктуальнаяДатаСеанса
						ТОГДА 6
					КОГДА ДокументЗаказНаПроизводство.Старт &lt;= КОНЕЦПЕРИОДА(&amp;АктуальнаяДатаСеанса, ДЕНЬ)
							И ДокументЗаказНаПроизводство.Финиш >= &amp;АктуальнаяДатаСеанса
						ТОГДА 4
					ИНАЧЕ 2
				КОНЕЦ
		КОГДА ЕСТЬNULL(ЗаказыНаПроизводствоОстаткиИОбороты.КоличествоКонечныйОстаток, 0) = 0
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ДокументЗаказНаПроизводство.СостояниеЗаказа = ЗНАЧЕНИЕ(Справочник.СостоянияЗаказовНаПроизводство.Завершен)
					ТОГДА 1
				КОГДА ДокументЗаказНаПроизводство.Финиш &lt; &amp;АктуальнаяДатаСеанса
					ТОГДА 5
				КОГДА ДокументЗаказНаПроизводство.Старт &lt;= КОНЕЦПЕРИОДА(&amp;АктуальнаяДатаСеанса, ДЕНЬ)
						И ДокументЗаказНаПроизводство.Финиш >= &amp;АктуальнаяДатаСеанса
					ТОГДА 3
				ИНАЧЕ 1
			КОНЕЦ
	КОНЕЦ КАК НомерКартинки,
	ВЫБОР
		КОГДА ДокументЗаказНаПроизводство.Финиш &lt; &amp;АктуальнаяДатаСеанса
				И ЕСТЬNULL(ЗаказыНаПроизводствоОстаткиИОбороты.КоличествоКонечныйОстаток, 0) > 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ПросроченоВыполнение,
	ВЫБОР
		КОГДА ДокументЗаказНаПроизводство.Старт &lt;= КОНЕЦПЕРИОДА(&amp;АктуальнаяДатаСеанса, ДЕНЬ)
				И ДокументЗаказНаПроизводство.Финиш >= &amp;АктуальнаяДатаСеанса
				И ЕСТЬNULL(ЗаказыНаПроизводствоОстаткиИОбороты.КоличествоКонечныйОстаток, 0) > 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК НаСегодня,
	&amp;АктуальнаяДатаСеанса,
	ДокументЗаказНаПроизводство.Ссылка,
	ДокументЗаказНаПроизводство.ПометкаУдаления,
	ДокументЗаказНаПроизводство.Номер,
	ДокументЗаказНаПроизводство.Дата,
	ДокументЗаказНаПроизводство.Проведен,
	ДокументЗаказНаПроизводство.ВидОперации,
	ДокументЗаказНаПроизводство.Организация,
	ДокументЗаказНаПроизводство.ЗаказПокупателя,
	ДокументЗаказНаПроизводство.Старт,
	ДокументЗаказНаПроизводство.Финиш,
	ДокументЗаказНаПроизводство.СтруктурнаяЕдиница,
	ДокументЗаказНаПроизводство.Комментарий,
	ДокументЗаказНаПроизводство.СтруктурнаяЕдиницаРезерв,
	ДокументЗаказНаПроизводство.ДокументОснование,
	ДокументЗаказНаПроизводство.ЗаказНаПроизводствоОснование КАК ЗаказНаПроизводство,
	ДокументЗаказНаПроизводство.СостояниеЗаказа,
	ДокументЗаказНаПроизводство.ВариантЗавершения,
	ДокументЗаказНаПроизводство.Ответственный,
	ДокументЗаказНаПроизводство.Автор,
	ВЫРАЗИТЬ(ДокументЗаказНаПроизводство.Заметки КАК СТРОКА(1024)) КАК Заметки,
	ДокументЗаказНаПроизводство.СписокНоменклатуры,
	ДокументЗаказНаПроизводство.СписокРесурсов,
	ДокументЗаказНаПроизводство.МоментВремени
ИЗ
