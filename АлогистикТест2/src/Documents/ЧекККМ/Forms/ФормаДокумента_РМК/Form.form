    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ДокументЧекККМ.Ссылка,
	ДокументЧекККМ.Номер,
	ДокументЧекККМ.Дата,
	ДокументЧекККМ.Архивный,
	ДокументЧекККМ.ВалютаДокумента,
	ДокументЧекККМ.ВидЦен,
	ДокументЧекККМ.КассаККМ,
	ДокументЧекККМ.КассоваяСмена,
	ДокументЧекККМ.Комментарий,
	ДокументЧекККМ.НДСВключатьВСтоимость,
	ДокументЧекККМ.НомерЧекаККМ,
	ДокументЧекККМ.Организация,
	ДокументЧекККМ.ПолученоНаличными,
	ДокументЧекККМ.СтруктурнаяЕдиница,
	ДокументЧекККМ.Статус,
	ДокументЧекККМ.СуммаДокумента,
	ДокументЧекККМ.ФормаОплаты,
	ДокументЧекККМ.НалогообложениеНДС,
	ДокументЧекККМ.СуммаВключаетНДС,
	ДокументЧекККМ.ЭквайринговыйТерминал,
	ДокументЧекККМ.ВидСкидкиНаценки,
	ДокументЧекККМ.Подразделение,
	ДокументЧекККМ.Автор,
	ДокументЧекККМ.Ответственный,
	ДокументЧекККМ.ДисконтнаяКарта,
	ДокументЧекККМ.ПроцентСкидкиПоДисконтнойКарте,
	ДокументЧекККМ.СкидкиРассчитаны,
	ДокументЧекККМ.МоментВремени,
	ВЫБОР
		КОГДА &amp;БезПодключенияОборудования
			ТОГДА ПОДСТРОКА(ДокументЧекККМ.Номер, 6, 6)
		ИНАЧЕ ДокументЧекККМ.НомерЧекаККМ
	КОНЕЦ КАК НомерЧека
ИЗ
	Документ.ЧекККМ КАК ДокументЧекККМ
ГДЕ
	ДокументЧекККМ.КассаККМ = &amp;КассаККМ
