
#Если Сервер Или ТолстыйКлиентОбычноеПриложение Или ВнешнееСоединение Тогда
	
	#Область ИнициализацияДаных
	
	// Инициализирует таблицы значений, содержащие данные табличных частей документа.
	// Таблицы значений сохраняет в свойствах структуры "ДополнительныеСвойства".
	//
	Процедура ИнициализироватьДанныеДокумента(ДокументСсылкаРеализация, СтруктураДополнительныеСвойства) Экспорт
		
		СформироватьТаблицаВыполнениеЗаказовЭтапыСерии(ДокументСсылкаРеализация, СтруктураДополнительныеСвойства);
		
		Запрос = Новый Запрос;
		Запрос.МенеджерВременныхТаблиц = СтруктураДополнительныеСвойства.ДляПроведения.СтруктураВременныеТаблицы.МенеджерВременныхТаблиц;
		Запрос.Текст = 
		"ВЫБРАТЬ
		|	алк_РеализацияПродукцииЭкспортЗапасы.Номенклатура,
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка.Дата КАК ДатаСобытия,
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка.Дата КАК Период,
		|	алк_РеализацияПродукцииЭкспортЗапасы.СерийныйНомер,
		|	ЗНАЧЕНИЕ(Перечисление.ОперацииСерийныхНомеров.Реализация) КАК Операция,
		|	алк_ПараметрыПакетовСрезПоследних.Характеристика
		|ИЗ
		|	Документ.алк_РеализацияПродукцииЭкспорт.Запасы КАК алк_РеализацияПродукцииЭкспортЗапасы
		|		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.алк_ПараметрыПакетов.СрезПоследних(, ) КАК алк_ПараметрыПакетовСрезПоследних
		|		ПО алк_РеализацияПродукцииЭкспортЗапасы.СерийныйНомер = алк_ПараметрыПакетовСрезПоследних.СерийныйНомер
		|ГДЕ
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка = &Ссылка";
		
		Запрос.УстановитьПараметр("Ссылка", ДокументСсылкаРеализация);
		РезультатЗапроса = Запрос.Выполнить();
		
		СтруктураДополнительныеСвойства.ТаблицыДляДвижений.Вставить("ТаблицаСерийныеНомераГарантии", РезультатЗапроса.Выгрузить());
		
	КонецПроцедуры // ИнициализироватьДанныеДокумента()
	
	#КонецОбласти
	
	
	#Область Проведение
	
	// Формирует таблицу значений, содержащую данные для проведения по регистру.
	// Таблицы значений сохраняет в свойствах структуры "ДополнительныеСвойства".
	//
	Процедура СформироватьТаблицаВыполнениеЗаказовЭтапыСерии(ДокументСсылкаРеализация, СтруктураДополнительныеСвойства)
		
		Запрос = Новый Запрос;
		Запрос.МенеджерВременныхТаблиц = СтруктураДополнительныеСвойства.ДляПроведения.СтруктураВременныеТаблицы.МенеджерВременныхТаблиц;
		Запрос.Текст =
		"ВЫБРАТЬ
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка.Организация,
		|	алк_РеализацияПродукцииЭкспортЗапасы.Номенклатура,
		|	алк_РеализацияПродукцииЭкспортЗапасы.СерийныйНомер,
		|	1 КАК КоличествоПакетов,
		|	&Период,
		|	&ЭтапВыполнения КАК ЭтапВыполненияЗаказа,
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка.Заказ КАК ЗаказПокупателя
		|ИЗ
		|	Документ.алк_РеализацияПродукцииЭкспорт.Запасы КАК алк_РеализацияПродукцииЭкспортЗапасы
		|ГДЕ
		|	алк_РеализацияПродукцииЭкспортЗапасы.Ссылка = &Ссылка";
		
		Запрос.УстановитьПараметр("Ссылка", ДокументСсылкаРеализация);
		Запрос.УстановитьПараметр("Период", ДокументСсылкаРеализация.Дата);
		Запрос.УстановитьПараметр("ЭтапВыполнения", Перечисления.алк_ЭтапыВыполненияЗаказов.Реализация);
		
		РезультатЗапроса = Запрос.Выполнить();  // РезультатЗапроса.Выгрузить()
		
		СтруктураДополнительныеСвойства.ТаблицыДляДвижений.Вставить("ТаблицаВыполнениеЗаказовЭтапыСерии", РезультатЗапроса.Выгрузить());
		
	КонецПроцедуры // СформироватьТаблицаВыполнениеЗаказовЭтапыСерии
	
	// Формирует таблицу значений, содержащую данные для проведения по регистру.
	// Таблицы значений сохраняет в свойствах структуры "ДополнительныеСвойства".
	//
	Процедура СформироватьТаблицаОтгрузкаРасходников(ДокументСсылкаТранспортировкаЗапасов, СтруктураДополнительныеСвойства)
		
		Запрос = Новый Запрос;
		Запрос.МенеджерВременныхТаблиц = СтруктураДополнительныеСвойства.ДляПроведения.СтруктураВременныеТаблицы.МенеджерВременныхТаблиц;
		Запрос.Текст =
		"ВЫБРАТЬ
		|	алк_ТранспортировкаЗапасов.ДокументОснование КАК ДокументРН,
		|	алк_ТранспортировкаЗапасов.ДокументыОснования.(
		|		ДокументОснование КАК ДокументОснованиеМультизаказ
		|	),
		|	алк_ТранспортировкаЗапасов.Ссылка КАК ДокументОтгрузки,
		|	&Период
		|ИЗ
		|	Документ.алк_ТранспортировкаЗапасов КАК алк_ТранспортировкаЗапасов
		|ГДЕ
		|	алк_ТранспортировкаЗапасов.Ссылка = &Ссылка";
		
		Запрос.УстановитьПараметр("Ссылка", ДокументСсылкаТранспортировкаЗапасов);
		Запрос.УстановитьПараметр("Период", ДокументСсылкаТранспортировкаЗапасов.Дата);
		
		РезультатЗапроса = Запрос.Выполнить();  // РезультатЗапроса.Выгрузить()
		
		ВыборкаШапка = РезультатЗапроса.Выбрать();
		
		Пока ВыборкаШапка.Следующий() Цикл
			
			Если ДокументСсылкаТранспортировкаЗапасов.Мультизаказ Тогда
				
				тзРезультат = Новый ТаблицаЗначений;
				тзРезультат.Колонки.Добавить("ДокументРН");
				тзРезультат.Колонки.Добавить("ДокументОтгрузки");
				тзРезультат.Колонки.Добавить("Период");
				
				ВыборкаДокументы = ВыборкаШапка.ДокументыОснования.Выбрать();
				
				Пока ВыборкаДокументы.Следующий() Цикл
					
					НоваяСтрока = тзРезультат.Добавить();
					НоваяСтрока.ДокументРН = ВыборкаДокументы.ДокументОснованиеМультизаказ;
					НоваяСтрока.ДокументОтгрузки = ВыборкаШапка.ДокументОтгрузки;
					НоваяСтрока.Период = ВыборкаШапка.Период;
					
				КонецЦикла; 
				
			Иначе
				
				тзРезультат = РезультатЗапроса.Выгрузить();
				
				Прервать;
				
			КонецЕсли; 
			
		КонецЦикла; 
		
		тзРезультат.Свернуть("ДокументРН, ДокументОтгрузки, Период");
		
		СтруктураДополнительныеСвойства.ТаблицыДляДвижений.Вставить("ТаблицаОтгрузкаРасходников", тзРезультат);
		
	КонецПроцедуры
	
	
	#КонецОбласти
	
	
	#Область ИнтерфейсПечати
	
	// Заполняет список команд печати.
	// 
	// Параметры:
	//   КомандыПечати - ТаблицаЗначений - состав полей см. в функции УправлениеПечатью.СоздатьКоллекциюКомандПечати.
	//
	Процедура ДобавитьКомандыПечати(КомандыПечати) Экспорт
		
		КомандаПечати = КомандыПечати.Добавить();
		КомандаПечати.Идентификатор = "Инвойс";
		КомандаПечати.Представление = НСтр("ru = 'Инвойс'");
		КомандаПечати.ПроверкаПроведенияПередПечатью = Истина;
		КомандаПечати.Порядок = 1;
		
		КомандаПечати = КомандыПечати.Добавить();
		КомандаПечати.Идентификатор = "СпецификацияПМ_Англ";
		КомандаПечати.Представление = НСтр("ru = 'Спецификация на английском'");
		КомандаПечати.ПроверкаПроведенияПередПечатью = Истина;
		КомандаПечати.Порядок = 2;
		
		КомандаПечати = КомандыПечати.Добавить();
		КомандаПечати.Идентификатор = "СпецификацияПМ_Англ_2";
		КомандаПечати.Представление = НСтр("ru = 'Спецификация на английском (вар.2)'");
		КомандаПечати.ПроверкаПроведенияПередПечатью = Истина;
		КомандаПечати.Порядок = 3;
		
	КонецПроцедуры
	
	// Сформировать печатные формы объектов
	//
	// ВХОДЯЩИЕ:
	//   ИменаМакетов    - Строка    - Имена макетов, перечисленные через запятую
	//   МассивОбъектов  - Массив    - Массив ссылок на объекты которые нужно распечатать
	//   ПараметрыПечати - Структура - Структура дополнительных параметров печати
	//
	// ИСХОДЯЩИЕ:
	//   КоллекцияПечатныхФорм - Таблица значений - Сформированные табличные документы
	//   ПараметрыВывода       - Структура        - Параметры сформированных табличных документов
	//
	Процедура Печать(МассивОбъектов, ПараметрыПечати, КоллекцияПечатныхФорм, ОбъектыПечати, ПараметрыВывода) Экспорт
		
		Если УправлениеПечатью.НужноПечататьМакет(КоллекцияПечатныхФорм, "Инвойс") Тогда
			
			УправлениеПечатью.ВывестиТабличныйДокументВКоллекцию(КоллекцияПечатныхФорм, "Инвойс", "Инвойс", ПечатнаяФорма(МассивОбъектов, ОбъектыПечати, "Инвойс"));
			
		КонецЕсли;
		
		Если УправлениеПечатью.НужноПечататьМакет(КоллекцияПечатныхФорм, "СпецификацияПМ_Англ") Тогда
			
			УправлениеПечатью.ВывестиТабличныйДокументВКоллекцию(КоллекцияПечатныхФорм, "СпецификацияПМ_Англ", "Спецификация на английском", ПечатнаяФорма(МассивОбъектов, ОбъектыПечати, "СпецификацияПМ_Англ"));
			
		КонецЕсли;
		
		Если УправлениеПечатью.НужноПечататьМакет(КоллекцияПечатныхФорм, "СпецификацияПМ_Англ_2") Тогда
			
			УправлениеПечатью.ВывестиТабличныйДокументВКоллекцию(КоллекцияПечатныхФорм, "СпецификацияПМ_Англ_2", "Спецификация на английском (вар.2)", ПечатнаяФорма(МассивОбъектов, ОбъектыПечати, "СпецификацияПМ_Англ_2"));
			
		КонецЕсли;
		
	КонецПроцедуры
	
	// Сформировать печатные формы объектов
	//
	Функция ПечатнаяФорма(МассивОбъектов, ОбъектыПечати, ИмяМакета)
		
		Перем Ошибки;
		
		ТабличныйДокумент = Новый ТабличныйДокумент;
		
		ПервыйДокумент = Истина;
		
		Для Каждого ТекущийДокумент Из МассивОбъектов Цикл
			
			Если Не ПервыйДокумент Тогда
				ТабличныйДокумент.ВывестиГоризонтальныйРазделительСтраниц();
			КонецЕсли;
			ПервыйДокумент = Ложь;
			
			НомерСтрокиНачало = ТабличныйДокумент.ВысотаТаблицы + 1;
			
			Если ИмяМакета = "Инвойс" Тогда
				
				СформироватьИнвойс(ТабличныйДокумент, ТекущийДокумент);
				
			КонецЕсли;
			
			Если ИмяМакета = "СпецификацияПМ_Англ" Тогда
				
				СформироватьСпецификацияАнгл(ТабличныйДокумент, ТекущийДокумент);
				
			КонецЕсли;
			
			Если ИмяМакета = "СпецификацияПМ_Англ_2" Тогда
				
				СформироватьСпецификацияАнгл_2(ТабличныйДокумент, ТекущийДокумент);
				
			КонецЕсли;
			
			ОбщегоНазначенияКлиентСервер.СообщитьОшибкиПользователю(Ошибки);
			
			Возврат ТабличныйДокумент;
			
		КонецЦикла
		
	КонецФункции // ПечатнаяФорма()
	
	// Процедура формирования печатной формы Инвойс
	//
	Процедура СформироватьИнвойс(ТабличныйДокумент, ТекущийДокумент)
		
		Макет = УправлениеПечатью.МакетПечатнойФормы("Документ.алк_РеализацияПродукцииЭкспорт.Инвойс");
		
		Запрос = Новый Запрос;
		Запрос.УстановитьПараметр("ТекущийДокумент", ТекущийДокумент);
		Запрос.Текст = 		
		"ВЫБРАТЬ
		|	алк_РеализацияПродукцииЭкспорт.Дата КАК ДатаДокумента,
		|	алк_РеализацияПродукцииЭкспорт.Номер,
		|	алк_РеализацияПродукцииЭкспорт.Организация.Префикс КАК Префикс,
		|	алк_РеализацияПродукцииЭкспорт.ТоварыОбъемныеГруппы.(
		|		НомерСтроки,
		|		Номенклатура,
		|		ПредставлениеОГ,
		|		Объем,
		|		Цена,
		|		Сумма
		|	),
		|	алк_РеализацияПродукцииЭкспорт.Продавец,
		|	алк_РеализацияПродукцииЭкспорт.Покупатель
		|ИЗ
		|	Документ.алк_РеализацияПродукцииЭкспорт КАК алк_РеализацияПродукцииЭкспорт
		|ГДЕ
		|	алк_РеализацияПродукцииЭкспорт.Ссылка = &ТекущийДокумент";
		
		
		Шапка = Запрос.Выполнить().Выбрать();
		Шапка.Следующий();
		
		ОбластьМакетаШапка = Макет.ПолучитьОбласть("Шапка");
		
		НомерДокумента = ПечатьДокументовУНФ.ПолучитьНомерНаПечатьСУчетомДатыДокумента(Шапка.ДатаДокумента, Шапка.Номер, Шапка.Префикс);
		
		ОбластьМакетаШапка.Параметры.Номер = НомерДокумента;
		ОбластьМакетаШапка.Параметры.Дата  = Формат(Шапка.ДатаДокумента, "ДЛФ=DD");
		
		Покупатель = РфпПолучениеДанныхСервер.АнглийскоеНаименованиеАдрес(Шапка.Покупатель);
		ОбластьМакетаШапка.Параметры.Покупатель = Покупатель;
		
		ТабличныйДокумент.Вывести(ОбластьМакетаШапка);
		
		ОбластьМакета = Макет.ПолучитьОбласть("Строка");
		
		ВыборкаСтрокТовары = Шапка.ТоварыОбъемныеГруппы.Выбрать();
		
		Сумма = 0;
		
		ЕстьДанныеДляВывода = ВыборкаСтрокТовары.Следующий();
		
		Пока ЕстьДанныеДляВывода Цикл
			
			ДанныеСтроки = ВыборкаСтрокТовары;
			
			ОбластьМакета.Параметры.Заполнить(ДанныеСтроки);	
			
			ТабличныйДокумент.Вывести(ОбластьМакета);
			
			Сумма = Сумма + ДанныеСтроки.Сумма;
			
			ЕстьДанныеДляВывода = ВыборкаСтрокТовары.Следующий();
			
		КонецЦикла;
		
		ОбластьМакета = Макет.ПолучитьОбласть("Итог");
		ОбластьМакета.Параметры.Сумма = Сумма;
		
		ТабличныйДокумент.Вывести(ОбластьМакета);
		
	КонецПроцедуры //СформироватьИнвойс()	
	
	// Процедура формирования печатной формы "Спецификация на английском"
	//
	Процедура СформироватьСпецификацияАнгл(ТабличныйДокумент, ТекущийДокумент)
		
		Запрос = Новый Запрос;
		Запрос.УстановитьПараметр("ТекущийДокумент", ТекущийДокумент);
		
		Запрос.Текст = 
		"ВЫБРАТЬ
		|	алк_РеализацияПродукцииЭкспорт.Номер,
		|	алк_РеализацияПродукцииЭкспорт.Дата,
		|	алк_РеализацияПродукцииЭкспорт.Заказ,
		|	ПРЕДСТАВЛЕНИЕ(алк_РеализацияПродукцииЭкспорт.Заказ)
		|ИЗ
		|	Документ.алк_РеализацияПродукцииЭкспорт КАК алк_РеализацияПродукцииЭкспорт
		|ГДЕ
		|	алк_РеализацияПродукцииЭкспорт.Ссылка = &ТекущийДокумент";
		
		РезультатШапка = Запрос.Выполнить().Выбрать();
		РезультатШапка.Следующий();
		
		Запрос.Текст = 
		"ВЫБРАТЬ
		|	алк_РеализацияПродукцииЭкспортЗапасы.Номенклатура КАК Номенклатура,
		|	алк_РеализацияПродукцииЭкспортЗапасы.СерийныйНомер,
		|	алк_РеализацияПродукцииЭкспортЗапасы.КоличествоПакетов КАК КоличествоПакетов,
		|	алк_РеализацияПродукцииЭкспортЗапасы.Объем КАК Объем,
		|	алк_РеализацияПродукцииЭкспортЗапасы.ОбъемТаможня КАК ОбъемТаможня,
		|	алк_РеализацияПродукцииЭкспортЗапасы.КоличествоСостав КАК КоличествоСостав,
		|	алк_РеализацияПродукцииЭкспортЗапасы.ВесНетто КАК ВесНетто,
